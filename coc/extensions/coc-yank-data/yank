c92aaff6a9f77afce238a5b0899999ee|file:///Users/aA/Desktop/chibi/cb/src/views/system/components/user.vue|408|1|V|vue
	    getRoleList() {
	      this.$axios
	        .get(`${this.URL_PREFIX}/user/role`, {
	          params: {
	            page: 1,
	            size: 10,
	            starttime: "",
	            endtime: "",
	            role: "",
	            status: "",
	            username: this.$store.state.user.username
	          }
	        })
	        .then(res => {
	          if (res.data.code == 0) {
	            for (let i in res.data.result) {
	              let nowrole = res.data.result[i];
	              let newrole = {};
	              newrole.id = nowrole.id;
	              newrole.name = nowrole.name;
	              newrole.desc = nowrole.remark || "";
	              newrole.status = nowrole.status;
	              this.roleList.push(newrole);
	            }
	          } else if (res.data.code == -1) {
	            this.$Message.error("请登录");
	          } else if (res.data.code == -2) {
	            this.$Message.error("无权访问");
	          } else {
	            this.$Message.error("数据加载出错...");
	          }
	        })
	        .catch(error => {
	          this.tableloadingrole = false;
	          this.$Message.loading("服务器维护中...");
	        });
	    },
