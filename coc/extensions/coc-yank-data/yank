	{
	    constructor() {
	        super()
	        this.state = {
	            currentSong: '卢巧音--- 垃圾'
	        }
	    }
	
	    playNextSong = () => {
	        this.setState({
	            currentSong: '卢巧音--- 喜欢恋爱'
	        })
	    }
	
	
	    render() {
	        return (
	            <Provider 
	                value={{
	                    data:this.state.currentSong,
	                    playNextSong:this.playNextSong
	                 }}
	             >
	                { this.props.children } 
	            </Provider> 
	        )
	    }
	}
