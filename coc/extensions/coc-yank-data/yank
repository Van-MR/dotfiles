	    //获取白名单
	    getWhiteList() {
	      this.tableLoading = true;
	      this.$axios
	        .get(
	          `${this.URL_PREFIX}/visitor/list?page=${this.currentPage}&size=${this.pageSize}&type=1&times=${this.times}&sex=${this.sex}`
	        )
