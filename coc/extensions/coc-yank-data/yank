	          align: "center",
	          render: (h, params) => {
	            let text = params.row.status;
	            let style = {};
	            switch (text) {
	              case "正常":
	                style.color = "#8ae41f";
	                break;
	              case "迟到":
	                style.color = "#f49731";
	                break;
	              case "早退":
	                style.color = "#fce533";
	                break;
	              default:
	                style.color = "#ed4c75";
	            }
	            return h("div", [
	              h(
	                "p",
	                {
	                  style: style,
	                  props: {
	                    type: "text"
	                  }
	                },
	                text
	              )
	            ]);
	          }
	        }
	      ]
	    };
	  },
	  created() {
	    this.pid = this.$route.params.id;
	    this.times = this.$route.query.times;
	    //动态获取子节点
	    this.backendMenuList.filter(item => {
	      if (item.name == "访客管理") {
	        this.MenuData[0].list = item.child;
	        //this.currentName = this.MenuData[0].list[0].name;
	      }
	    });
	  },
	  computed: {
	    ...mapState({
	      backendMenuList: state => state.app.backendMenuList
	    })
	  },
	  mounted() {
	    this.fetchAttendanceData(this.pid, this.times);
	    //this.fetchAttendanceList(this.pid);
	    this.getPersonAttendanceList();
	  },
	  methods: {
	    handleDateSelector(time) {
	      this.queryDateTime = time;
	    },
	    confirmDate() {
	      this.spinShow = true;
	      this.fetchAttendanceData(this.pid, this.queryDateTime);
	    },
	    changeFoldState(item, $event) {
	      this.changeColorIndex = 0;
	      this.brandFold = !this.brandFold;
	    },
	    addActive(item, event) {
	      this.currentName = item.name;
	      this.$router.push({
	        path: "/visitors/index",
	        query: {
	          currentname: this.currentName
	        }
	      });
	    },
	    //用后端数据生成表格头结构
	    generateColumns(initColumns, renderColumns) {
	      renderColumns.forEach((thead, index) => {
	        initColumns[index].key = thead.key;
	        initColumns[index].title = thead.title;
	      });
	    },
	
	    //top part personla attendance statistic data
	    fetchAttendanceData(id, times = "") {
	      this.$axios
	        .get(`${this.URL_PREFIX}/statistic/person?id=${id}&times=${times}`)
	        .then(res => {
	          if (res.data.code === "0") {
	            const {
	              person_info,
	              month,
	              quarter,
	              punth_time
	            } = res.data.result[0];
	            this.personInfo = person_info[0].info[0];
	            this.personStasticInfo = person_info[0].statistic_info;
	            this.monthInfo = month[0];
	            this.seasonInfo = quarter[0];
	            this.attendanceTimeInfo = punth_time;
	            this.lineChartData.xData = person_info[0].quarter_data.map(
	              item => item.name
	            );
	            this.lineChartData.expectedData = person_info[0].quarter_data.map(
	              item => item.total
	            );
	            //loading
	            setTimeout(() => {
	              this.spinShow = false;
	            }, 500);
	          }
	        })
	        .catch(error => {
	          console.log(error);
	        });
	    },
	
	    getPersonAttendanceList() {
	      this.loading = true;
	      this.$axios
	        .get(
	          `${this.URL_PREFIX}/statistic/person/list?id=${this.pid}&time=${this.times}&page=${this.currentPage}&size=${this.pageSize}`
	        )
	        .then(res => {
	          if (res.data.code === "0") {
	            this.loading = false;
	            console.log(res.data.result);
	          } else {
	            this.loading = false;
	            this.$Message.error(res.data.msg);
	          }
	        })
	        .catch(err => {
	          this.loading = false;
	          console.log(err);
	        });
	    },
	
	    //handle page changeEvent
	    changePage(index) {
	      this.currentPage = index;
	      this.fetchAttendanceList(this.pid);
	    }
	  }
	};
	
