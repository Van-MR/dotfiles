435c46db183d44b2ac47588805bd4be9|file:///Users/aA/Desktop/chibi/cb/src/views/cam/index.vue|1|1|V|vue
	<template>
	  <div class="main">
	    <div class="media">
	      <div class="player">
	        <video-player class="vjs-custom-skin" :options="playerOptions">
	        </video-player>
	      </div>
	    </div>
	    <div class="facelogs" v-if="shouldReforceUpdate">
	      <ul v-for="(item, index) in faceLogs" :key="index">
	        <div class="left">
	          <img :src="item.pic" alt="pic" />
	        </div>
	        <div class="right">
	          <li>
	            <span class="label">姓名:</span>
	            <span>{{ item.name }}</span>
	          </li>
	          <li>
	            <span class="label">年龄:</span>
	            <span>{{ item.age }}</span>
	          </li>
	          <li>
	            <span class="label">性别:</span>
	            <span>{{ item.sex }}</span>
	          </li>
	          <li>
	            <span class="label">所属部门:</span>
	            <span>{{ item.group }}</span>
	          </li>
	          <li>
	            <span class="label">所属人群:</span>
	            <span>{{ item.type }}</span>
	          </li>
	          <li v-show="item.hal_status !== '0'">
	            <span class="label">安全帽:</span>
	            <span>{{ item.hal_status }}</span>
	          </li>
	          <li v-show="item.smoke_status !== '0'">
	            <span class="label">吸烟:</span>
	            <span>{{ item.smoke_status }}</span>
	          </li>
	          <li>
	            <span class="label">通行时间:</span>
	            <span>{{ item.posttime.split(" ")[1] }}</span>
	          </li>
	        </div>
	        <!--左上边框-->
	        <div class="line-box">
	          <i class="t-l-line"></i>
	          <i class="l-t-line"></i>
	        </div>
	        <!--右上边框-->
	        <div class="line-box">
	          <i class="t-r-line"></i>
	          <i class="r-t-line"></i>
	        </div>
	        <!--左下边框-->
	        <div class="line-box">
	          <i class="l-b-line"></i>
	          <i class="b-l-line"></i>
	        </div>
	        <!--右下边框-->
	        <div class="line-box">
	          <i class="r-b-line"></i>
	          <i class="b-r-line"></i>
	        </div>
	      </ul>
	    </div>
	    <option-tool
	      :camList="channelList"
	      :switchCam="switchCam"
	      :switchAlgorithmType="changeAlgorithmType"
	    />
	  </div>
	</template>
	
	<script>
	import "./cam.css";
	import "videojs-flash";
	import optionTool from "./components/switchScene.vue";
	
	export default {
	  components: { optionTool },
	  data() {
	    return {
	      type: "0",
	      currentIP: "",
	      showMenuList: false,
	      channelList: [],
	      currentIndex: 0,
	      faceLogs: [],
	      shouldReforceUpdate: true,
	      offsetIndex: 0,
	      timer: null,
	      countTime: 0,
	      noPasser: false,
	      playerOptions: {
	        sources: [
	          {
	            type: "rtmp/mp4",
	            src: ""
	          }
	        ],
	        techOrder: ["flash"],
	        flash: {
	          swf: require("../../static/swf/video-js.swf")
	        },
	        autoplay: true,
	        controls: false,
	        poster: require("../../assets/images/camposter.jpg")
	      }
	    };
	  },
	  created() {
	    this.$store.commit("toggleIconMenu");
	  },
	  mounted() {
	    this.getChanels();
	    this.initWebSocket();
	    window.onbeforeunload = e => this.beforeunloadHandler(e);
	  },
	
	  watch: {
	    //30s内无新抓拍数据返回,清空抓拍列表
	    countTime(val) {
	      if (this.countTime === 30) {
	        this.faceLogs = [];
	        this.closeTimer();
	      }
	    }
	  },
	  methods: {
	    //切换不同失败场景 人脸 抽烟 安全帽
	    changeAlgorithmType(type) {
	      this.type = type;
	      this.loadAlgorithm();
	    },
	
	    //加载算法
	    loadAlgorithm(ip) {
	      this.$axios
	        .post(`${this.URL_PREFIX}/device/video/label`, {
	          ip: this.currentIP,
	          type: this.type
	        })
	        .then(res => {
	          if (res.data.code === "0") {
	            this.$Message.success(res.data.msg);
	          } else {
	            this.$Message.error(res.data.msg);
	          }
	        })
	        .catch(err => {
	          this.$Message.error("服务器出出错...");
	          console.log(err);
	        });
	    },
	
	    //视频推流
	    fetchUpstream(ip) {
	      return new Promise((resolve, reject) => {
	        this.$axios
	          .post(`${this.URL_PREFIX}/device/video/rtmp`, {
	            ip
	          })
	          .then(res => {
	            if (res.data.code === "0") {
	              resolve(res.data.url);
	            }
	          })
	          .catch(err => {
	            reject(err);
	            this.$Message.error("服务器出错");
	          });
	      });
	    },
	
	    //获取频道列表
	    getChanels() {
	      this.$axios
	        .get(`${this.URL_PREFIX}/device/video/list?keyword=`)
	        .then(res => {
	          if (res.data.code === "0") {
	            this.channelList = res.data.result;
	            this.currentIP = this.channelList[0].ip;
	            this.loadAlgorithm(this.currentIP);
	            this.fetchUpstream(this.currentIP).then(url => {
	              this.playerOptions.sources[0].src = url;
	            });
	          }
	        })
	        .catch(err => {
	          console.log(err);
	        });
	    },
	
	    //切换通道
	    switchCam(event, item, index) {
	      this.currentIP = item.ip;
	      this.currentIndex = index;
	      this.loadAlgorithm(this.currentIP);
	      this.fetchUpstream(this.currentIP).then(url => {
	        this.playerOptions.sources[0].src = url;
	      });
	    },
	
	    initWebSocket() {
	      const wsurl = `${this.URL_WS1}`;
	      this.websock = new WebSocket(wsurl);
	      this.websock.onopen = this.websocketopen;
	      this.websock.onmessage = this.websocketonmessage;
	      this.websock.onclose = this.websocketclose;
	    },
	
	    beforeunloadHandler(e) {
	      this.websock.send("close");
	    },
	
	    loadTimer() {
	      this.timer = setInterval(() => {
	        this.countTime++;
	      }, 1000);
	    },
	
	    closeTimer() {
	      clearInterval(this.timer);
	      this.countTime = 0;
	    },
	
	    //打开
	    websocketopen() {
	      this.websock.send("start");
	    },
	
	    //数据发送
	    websocketsend() {
	      //this.websock.send("start");
	    },
	
	    websocketclose() {
	      //this.websock.send("close");
	    },
	
	    //数据接收
	    websocketonmessage(e) {
	      let res = JSON.parse(e.data);
	      let result = res.result;
	      this.websock.send("start");
	      if (result.length > 0 && !res.history) {
	        this.shouldReforceUpdate = false;
	        this.closeTimer();
	        //对象destructor 赋值
	        let {
	          name,
	          age,
	          type,
	          sex,
	          posttime,
	          group,
	          pic,
	          hal_status,
	          smoke_status
	        } = res.result[0];
	
	        //当前返回的该条记录
	        let logItem = {
	          name,
	          age,
	          sex,
	          posttime,
	          group,
	          type,
	          pic: pic[0],
	          hal_status,
	          smoke_status
	        };
	
	        //查找当前最新返回的记录是否存在于记录集当中
	        let exitIndex = this.faceLogs.findIndex(
	          log => log.name === logItem.name
	        );
	
	        //如果最新返回的记录之前存在,就替换掉之前同名的那条记录
	        if (exitIndex > -1) {
	          this.faceLogs[exitIndex] = logItem;
	          this.shouldReforceUpdate = true;
	          this.loadTimer();
	        } else {
	          //如果不存在,就往记录集当中添加,同时当卡片数超过3个时,新的记录按当前卡片排序依次进行替换
	          this.offsetIndex++;
	          if (this.offsetIndex < 4) {
	            this.faceLogs = [...this.faceLogs, logItem];
	            this.shouldReforceUpdate = true;
	          } else {
	            let remain_Index = this.offsetIndex % 3;
	            remain_Index = remain_Index === 0 ? 3 : remain_Index;
	            this.faceLogs[remain_Index - 1] = logItem;
	            this.shouldReforceUpdate = true;
	          }
	          this.loadTimer();
	        }
	
	        //记录超出屏幕时,自动滚动展示
	        //if (this.faceLogs.length > 3) {
	        //  var target = document.getElementsByClassName("facelogs")[0];
	        //  target.style.transform = `translateY(${-240 *
	        //    (this.offsetIndex - 3)}px)`;
	        //}
	      }
	    }
	  },
	
	  beforeDestroy() {
	    //离开页面时关闭websocket连接
	    this.websock.send("close");
	    this.closeTimer();
	  }
	};
	</script>
