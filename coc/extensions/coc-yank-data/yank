e0ddb252f59d10f645d6ac27ec9b05c3|file:///Users/aA/Desktop/chibi/cb/src/views/equipment/components/equipList.vue|559|1|V|vue
	    //头像上传钩子函数
	    handleBeforeUpload(file) {
	      this.avatar = file;
	      this.uploadAvatar(this.avatar);
	    },
eb89d4ebcb387156188cb90d0b4268a7|file:///Users/aA/Desktop/chibi/cb/src/views/equipment/components/equipList.vue|559|1|V|vue
	    //人员头像上传
	    uploadAvatar(avatar) {
	      let oForm = new FormData();
	      oForm.append("files", avatar);
	      let config = {
	        headers: {
	          "Content-Type": "multipart/form-data"
	        }
	      };
	      this.$axios
	        .post(`${this.URL_PREFIX}/upload/pic`, oForm, config)
	        .then(res => {
	          if (res.data.code === "0") {
	            this.avatar = "";
	            this.equipAddForm.topic = res.data.url;
	          }
	        })
	        .catch(err => {
	          console.log(err);
	          this.avatar = "";
	          this.$Message.error("服务器出错...");
	        });
	    },
