7953f76c95cc939d67414459834222e9|file:///Users/aA/Downloads/custom_ui/src/components/Spin/index.js|1|1|V|javascript
	import React from 'react'
	import './index.css'
	
	export default function Spin(props) {
	  const {
	    isLoading = true,
	    type,
	    loadingText = '正在加载中。。。',
	    hiddeText = true,
	    bgColor = '#06c'
	  } = props
	
	  const spinType = {
	    line: 'line'
	  }
	
	  return isLoading ? (
	    <div className="loader">
	      <div
	        className={`inner ball-spin-fade-loader${
	          spinType[type] ? ' ' + spinType[type] : ''
	        }`}
	        style={{ backgroundColor: bgColor }}
	      >
	        <div></div>
	        <div></div>
	        <div></div>
	        <div></div>
	        <div></div>
	        <div></div>
	        <div></div>
	        <div></div>
	      </div>
	      {!hiddeText && <p className="loading-text">{loadingText}</p>}
	    </div>
	  ) : null
	}
