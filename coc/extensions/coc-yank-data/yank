	var reverse = function(x) {
	  if (x > 0 && x < Math.pow(2, 31)) {
	    var s = x + "";
	    var r = s
	      .split("")
	      .reverse()
	      .join("");
	    var o = parseInt(r.replace(/^0*/g, ""));
	    if (o > Math.pow(2, 31) - 1) {
	      o = 0;
	    }
	    console.log("c", o);
	    return o;
	  } else if (x > Math.pow(-2, 31) && x < 0) {
	    console.log("testing");
	    var s = x + "";
	    var r = s
	      .replace(/^-/, "")
	      .split("")
	      .reverse()
	      .join("");
	    var o = `-${parseInt(r.replace(/^0*/g, ""))}`;
	    console.log("aa", o);
	    return o;
	  } else {
	    x = 0;
	    console.log(x);
	    return x;
	  }
	};
	
	reverse(1534236469);
