b12b514ba63c2d2c21ace45c7eded229|file:///Users/aA/Desktop/epidemic-prevention/src/libs/getGeo.js|1|1|V|javascript
	export const IsWeixinOrAlipay = () => {
	  var ua = window.navigator.userAgent.toLowerCase();
	  //判断是不是微信
	  if (ua.match(/MicroMessenger/i) === "micromessenger") {
	    return "WeiXIN";
	  }
	  //判断是不是支付宝
	  if (ua.match(/AlipayClient/i) === "alipayclient") {
	    return "Alipay";
	  }
	  //哪个都不是
	  return "false";
	};
