	var firstUniqChar = function(s) {
	  var Arr = s.split("");
	  var hash = {};
	
	  for (var i = 0; i < Arr.length; i++) {
	    hash[Arr[i]] ? hash[Arr[i]]++ : (hash[Arr[i]] = 1);
	  }
	
	  console.log(hash);
	
	  var x = Object.entries(hash).filter(arr => {
	    if (arr.includes(1)) {
	      return arr;
	    }
	  });
	
	  if (x.length === 0) {
	    return -1;
	  }
	
	  console.log(x);
	
	  var c = x[0][0];
	
	  var k = s.indexOf(c);
	  console.log(k);
	
	  return k;
	};
	
	firstUniqChar("cc");
