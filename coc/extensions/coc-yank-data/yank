	  //从微信端扫码 获取code
	  const getCode = useCallback(() => {
	    var code = getParams();
	    if (code) {
	      id = id.split("&")[0];
	    }
	
	    getOpenId(code);
	
	    if (id && code) {
	      getQrCodeInfo(id);
	      fetchUserInfo();
	    }
	  }, [fetchUserInfo, getOpenId, getQrCodeInfo]);
8e53f00ed89117111813ffb8d1eddf6a|file:///Users/aA/Desktop/epidemic-prevention/src/pages/Entry.js|200|1|V|javascript
	  const getOpenId = async (code) => {
	    try {
	      const res = await fetch(`${BASE_URL}/phone/wechat/access?code=${code}`);
	      const jsonData = await res.json();
	      if (jsonData.code === "0") {
	        localStorage.setItem("openId", JSON.stringify(jsonData.only_id));
	        setToken(jsonData.token);
	        setOnlyID(jsonData.only_id);
	      } else if (jsonData.code === "-10011") {
	        localStorage.removeItem("openId");
	        //没有openId 重定向到授权页面
	        window.location.replace(
	          "https://open.weixin.qq.com/connect/oauth2/authorize?appid=" +
	            jsonData.only_id +
	            "&redirect_uri=" +
	            encodeURIComponent(window.location.href) +
	            "&response_type=code&scope=snsapi_userinfo&state=STATE#wechat_redirect"
	        );
	      }
	    } catch (e) {
	      console.log(e);
	    }
	  };
