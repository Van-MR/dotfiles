ce87f805bcb36d70c704d7a99bcdc6a7|file:///Users/aA/Desktop/chibi/cb/src/views/visitors/components/history.vue|359|1|V|vue
	    getVisitorList() {
	      this.loading = true;
	      this.$axios
	        .get(
	            `${this.URL_PREFIX}/visitors/list?page=${this.currentPage}&size=${this.pageSize}&keyword=${this.keyword}&type=${this.visit_type || ''}&starttime=${this.startTime}&endtime=${this.endTime}`
	        )
	        .then(res => {
	          if (res.data.code == "0") {
	            this.loading = false;
	            this.data1 = res.data.result;
	            this.totalCount = res.data.total;
	            if (this.data1.length < 1) {
	              this.loading = false;
	              this.$Message.warning("未搜索到匹配信息,请重试!");
	              this.totalCount = 0;
	            }
	          }
	        })
	        .catch(error => {
	          this.loading = false;
	          console.log(error);
	        });
	    },
