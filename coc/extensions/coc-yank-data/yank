	      if (this.loginForm.username == "" || this.username == null) {
	        this.$Message.warning("请输入用户名");
	      } else if (this.passwd == "" || this.passwd == null) {
	        this.$Message.warning("请输入登录密码");
	      } else {
e82f3e56d2a6718da3f068e5e6f91343|file:///Users/aA/Desktop/chibi/cb/src/views/login.vue|137|7|V|vue
	      }
0bc874aeb6a780de762758075c4964e0|file:///Users/aA/Desktop/chibi/cb/src/views/login.vue|85|1|V|vue
	      if (this.loginForm.username == "" || this.username == null) {
	        this.$Message.warning("请输入用户名");
	      } else if (this.passwd == "" || this.passwd == null) {
	        this.$Message.warning("请输入登录密码");
	      } else {
	        this.$axios
	          .post(`${this.URL_PREFIX}/login`, this.loginForm)
	          .then(res => {
	            if (res.data.code == "0") {
	              //首次登录成功后浏览器保存用户账号和密码信息
	              Cookies.set("user", this.username);
	              Cookies.set("passwd", this.passwd);
	
	              setTimeout(() => {
	                this.$Notice.success({
	                  title: "消息提示",
	                  render: h => {
	                    return h("span", ["用户登录 ", h("a", "成功")]);
	                  }
	                });
	              }, 500);
	              this.$store.commit("saveUser", this.username);
	              //保存token 存入vuex
	              this.$store.commit("saveToken", res.data.token);
	              this.getAccessMenuList(res.data.uid);
	              setTimeout(() => {
	                this.$router.push({ path: "/visitors/index" });
	              }, 500);
	            } else if (res.data.code == "10003") {
	              setTimeout(() => {
	                this.$Notice.error({
	                  title: "消息提示",
	                  render: h => {
	                    return h("span", ["该用户 ", h("a", "未启用")]);
	                  }
	                });
	              }, 500);
	            } else {
	              setTimeout(() => {
	                this.$Notice.error({
	                  title: "消息提示",
	                  render: h => {
	                    return h("span", [
	                      "登录失败!",
