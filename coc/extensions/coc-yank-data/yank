	            this.generateColumns(this.columns1, table_header);
	            this.data1 = table;
	            this.totalCount = res.data.total;
	            this.renderTable = true;
	            this.lineChartData.xData = chart[0].times;
	            this.lineChartData.expectedData = chart[0].info;
	            this.lineChartData.legendData = chart[0].type;
	          }
	        })
	        .catch(err => {
	          this.tableLoading = false;
	          console.log(err);
	        });
	    },
