	    /*
	     * @method { PUT}
	     * @description 修改访客黑白名单状态
	     */
	    changeStatus(visitor_id, status) {
	      this.$axios
	        .put(
	          `${this.URL_PREFIX}/visitor/status?id=${visitor_id}&status=${status}`
	        )
	        .then(res => {
	          if (res.data.code === "0") {
	            this.$Message.success("修改成功");
	            this.getWhiteList();
	          }
	        })
	        .catch(err => {
	          console.log(err);
	        });
	    },
