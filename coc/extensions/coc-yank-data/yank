	    const getOpenId = async (code = "") => {
	        try {
	            const res = await fetch(`${baseURL}/wx/user/access?code=${code}`)
	            const jsonData = await res.json();
	            if (jsonData.code === '0') {
	                localStorage.setItem('openId', JSON.stringify(res.only_id))
	            } else if (jsonData.code === '-10011') {
	                //重定向到授权页面
	                window.location.replace("https://open.weixin.qq.com/connect/oauth2/authorize?appid=" + jsonData.only_id + "&redirect_uri=" + encodeURIComponent(window.location.href) + '&response_type=code&scope=snsapi_userinfo&state=STATE#wechat_redirect');
	            }
	        } catch (e) {
	            console.log(e)
	        }
	    }
cadf771b7426f030fc2068188f5cb8eb|file:///Users/aA/Desktop/face_mobile/src/pages/Visitor/index.js|66|1|V|javascript
	    const getParams = () => {
	        let code;
	        let location = window.location.href;
	        if (location.includes('code=')) {
	            let k = location.split("?")[1];
	            let d = k.split("&")[0];
	            let f = d.substr(0, 5);
	            code = d.replace(f, '')
	            return code
	        }
	
	        code = ''
	        return code;
	    }
