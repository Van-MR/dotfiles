783865dba18f7b4c260b93c0b817863f|file:///Users/aA/Desktop/chibi/cb/src/views/event/eventInfo.vue|55|1|v|vue
	<script>
	import "../home/index.scss";
	import CountTo from "../my-components/count-to/CountTo.vue";
	import eventPie from "../home/components/visiting.vue";
	export default {
	  name: "eventInfo",
	  components: { CountTo, eventPie },
	  data() {
	    return {
	      video_id: null,
	      tableLoading: false,
	      pageSize: 8,
	      currentPage: 1,
	      totalCount: 0,
	      statistics: [],
	      countStyle1: {
	        fontSize: "35px",
	        color: "#92f121"
	      },
	      countStyle2: {
	        fontSize: "35px",
	        color: "#fde633"
	      },
	      countStyle3: {
	        fontSize: "35px",
	        color: "#f49731"
	      },
	      pics: [],
	      query: {
	        keyword: "",
	        types: "",
	        times: "",
	        currentPage: 1,
	        pageSize: 8
	      },
	      columns1: [
	        {
	          title: "序号",
	          key: "id",
	          width: 80,
	          align: "center"
	        },
	        {
	          title: "事件名称",
	          key: "name",
	          align: "center"
	        },
	        {
	          title: "算法类型",
	          key: "type",
	          align: "center"
	        },
	        {
	          title: "设备",
	          key: "device",
	          align: "center"
	        },
	        {
	          title: "分析状态",
	          key: "status",
	          align: "center"
	        },
	        {
	          title: "分析时间",
	          key: "times",
	          align: "center"
	        },
	        {
	          title: "操作",
	          align: "center",
	          render: (h, params) => {
	            return h("div", [
	              h(
	                "p",
	                {
	                  style: {
	                    color: "#258ee9",
	                    cursor: "pointer"
	                  },
	                  on: {
	                    click: () => {}
	                  }
	                },
	                "重新分析"
	              ),
	              h(
	                "p",
	                {
	                  style: {
	                    color: "#258ee9",
	                    cursor: "pointer"
	                  },
	                  on: {
	                    click: () => {
	                      let eventId = params.row.id;
	                      this.$router.push({
	                        path: "/event/info",
	                        params: eventId
	                      });
	                    }
	                  }
	                },
	                "查看详情"
	              )
	            ]);
	          }
	        }
	      ],
	      data1: [],
	      //通行统计饼图
	      pieChartData: []
	    };
	  },
	  methods: {
	    getAnalysInfo(id) {
	      this.$axios
	        .get(
	          `${this.URL_PREFIX}/device/video/file/statistic?video_id=${this.video_id}`
	        )
	        .then(res => {
	          if (res.data.code === "0") {
	            const { event, rate } = res.data.result[0];
	            this.statistics = event;
	            this.pieChartData = rate;
	          }
	        })
	        .catch(err => {
	          console.log(err);
	        });
	    },
	
	    getAnalyPics(id) {
	      this.$axios
	        .get(
	          `${this.URL_PREFIX}/device/video/file/statistic/pic?video_id=${this.video_id}&page=${this.currentPage}&size=${this.pageSize}`
	        )
	        .then(res => {
	          if (res.data.code === "0") {
	            this.pics = res.data.result;
	            this.totalCount = res.data.total;
	          }
	        })
	        .catch(err => {
	          console.log(err);
	        });
	    },
	
	    changepage(index) {
	      this.currentPage = index;
	      this.getAnalyPics();
	    }
	  },
	  mounted() {
	    this.video_id = this.$route.params.id;
	    this.getAnalysInfo(this.video_id);
	    this.getAnalyPics(this.video_id);
	  }
	};
	</script>
