	        .get(`${this.URL_PREFIX}/statistic/shifts/dict?id=1`)
	        .then(res => {
	          if (res.data.code === "0") {
	            let scheme = res.data.result[0].name;
	            let id = res.data.result[0].id;
	
	            //工作日
	            for (let i = 0; i < 5; i++) {
	              this.data[i].times = scheme;
	              this.data[i].id = id;
	              this.data[i].index = i;
	            }
	            //周末
	            for (let i = 5; i < 7; i++) {
	              this.data[i].id = 0;
	              this.data[i].index = i;
	              this.data[i].times = "休息";
	            }
	          }
	        })
	        .catch(err => {
	          console.log(err);
	        });
