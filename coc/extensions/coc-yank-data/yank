	       var renderer, scene, camera, light, geometry, material
	
	       function initRenderer() {
	           renderer = new THREE.WebGLRenderer({antialias: true})
	           renderer.setSize(window.innerWidth, window.innerHeight)
	           document.body.appendChild(renderer.domElement)
	       }
	
	       function initScene() {
	           scene = new THREE.Scene()
	       }
	
	       function initCamera() {
	           camera = new THREE.PerspectiveCamera(45,window.innerWidth/window.innerHeight,1,1000)
	           camera.position.x=0
	           camera.position.y=1000
	           camera.position.z=0
	           camera.up.set(0,0,1)
	           camera.lookAt(0,0,0)
	       }
	
	       function initLight() {
	           light = new THREE.DirectionalLight(0xFF0000,1.0,0)
	           light.position.set(100,100,200)
	           scene.add(light)
	       }
	
	       function initObject() {
	           geometry = new THREE.Geometry()
	           geometry.vertices.push(
	               new THREE.Vector3(-500,0,0),
	               new THREE.Vector3(500,0,0)
	           )
	           material = new THREE.LineBasicMaterial({color: 0xfffff,opacity: 0.5})
	           for (let index = 0;index<20;index++) {
	               var lineX = new THREE.Line(geometry,material)
	               lineX.position.z = (index * 50) - 500
	               scene.add(lineX)
	
	               var lineZ = new THREE.Line(geometry,material)
	               lineZ.position.x = (index*50) - 500
	               //旋转90度  沿Y轴
	               lineZ.rotation.y = 90 * Math.PI/180
	               scene.add(lineZ)
	           }
	       }
	
	       function render() {
	           renderer.clear()
	           renderer.render(scene,camera)
	       }
	
	       function init() {
	           initRenderer()
	           initCamera()
	           initScene()
	           initLight()
	           initObject()
	           render()
	       }
	
	       init()
	   
