	                params.row.status
	              ),
	              h(
	                "button",
	                {
	                  style: {
	                    display: "inline",
	                    marginleft: "8px",
	                    display:
	                      (params.row.status === "旷课") | "缺勤"
	                        ? "inline"
	                        : "none"
	                  },
	                  props: {
	                    type: "primary",
	                    size: "small"
	                  },
	                  on: {
	                    click: () => {
	                      this.$axios
	                        .post(`${this.url_prefix}/attendance/group/ctrl`, {
	                          id: params.row.id
	                        })
	                        .then(res => {
	                          if (res.data.code === "0") {
	                            this.showSubjectInfoModal = false;
	                            this.$message.success("补签成功");
	                          }
	                        })
	                        .catch(err => {
	                          this.$message.success("服务器出错");
	                          console.log(err);
	                        });
	                    }
	                  }
	                },
	                "补签"
	              )
	            ]);
	          }
	        }
	      ]
