	import { mapState, mapActions } from 'vuex'
	import CountTo from "../../my-components/count-to/CountTo";
	export default {
	  data() {
	    return {
	      tableLoading: false,
	      searchLoading: false,
	      export_status: false,
	      showTrafficInfo: false,
	      directions: [{ id: 0, name: '进门' }, { id: 1, name: '出门' }],
	      trafficInfo: {},
	      countStyle1: {
	        fontSize: "35px",
	        color: "#92f121"
	      },
	      countStyle2: {
	        fontSize: "35px",
	        color: "#fde633"
	      },
	      countStyle3: {
	        fontSize: "35px",
	        color: "#f49731"
	      },
	      query: {
	        keyword: '',
	        direction: '',
	        times: '',
	        currentPage: 1,
	        pageSize: 8
	      },
	      totalCount: 0,
	      data1: [],
	      columns1: [
	        {
	          title: '序号',
	          key: 'id',
	          width: 80,
	          align: 'center'
	        },
	        {
	          title: '照片',
	          key: 'pic',
	          width: 100,
	          align: 'center',
	          render: (h, params) => {
	            let src = params.row.pic
	            return h('div', [
	              h('img', {
	                attrs: {
	                  src
	                },
	                style: {
	                  width: '45px',
	                  height: '45px',
	                  cursor: 'pointer',
	                  marginTop: '5px'
	                }
	              })
	            ])
	          }
	        },
	        {
	          title: '姓名',
	          key: 'name',
	          align: 'center'
	        },
	        {
	          title: '身份',
	          key: 'type',
	          align: 'center'
	        },
	        {
	          title: '设备名称',
	          key: 'device',
	          align: 'center'
	        },
	        {
	          title: '进出方向',
	          key: 'direction',
	          align: 'center'
	        },
	        {
	          title: '通行时间',
	          key: 'times',
	          align: 'center'
	        },
	        {
	          title: '操作',
	          align: 'center',
	          render: (h, params) => {
	            return h('div', [
	              h(
	                'p',
	                {
	                  style: {
	                    color: '#258ee9',
	                    cursor: 'pointer'
	                  },
	                  on: {
	                    click: () => {
	                      this.getTrafficInfo(params.row.id)
	                    }
	                  }
	                },
	                '查看详情'
	              )
	            ])
	          }
	        }
	      ],
	      data1: [
	        {
	          id: 1,
	          pic: '',
	          name: '李文',
	          identity: '员工',
	          equip: '门禁机',
	          direction: '西南',
	          time: '2019-02-20 10:30:29'
	        },
	        {
	          id: 2,
	          pic: '',
	          name: '张华',
	          identity: '员工',
	          equip: '大门',
	          direction: '西南',
	          time: '2019-02-20 10:30:29'
	        },
	        {
	          id: 2,
	          pic: '',
	          name: '王条',
	          identity: '员工',
	          equip: '门禁机',
	          direction: '西南',
	          time: '2019-02-20 10:30:29'
	        }
	      ]
	    }
	  },
	  mounted() {
	    this.getTrafficList()
	  },
	  methods: {
	    getTrafficList() {
	      this.tableLoading = true
	      this.$axios
	        .get(
	          `${this.URL_PREFIX}/current/log?page=${this.query.currentPage}&size=${
	            this.query.pageSize
	          }&keyword=${this.query.keyword || ''}&direction=${this.query
	            .direction || ''}&times=${this.query.times || ''}`
	        )
	        .then(res => {
	          if (res.data.code == '0') {
	            this.tableLoading = false
	            this.data1 = res.data.result
	            this.totalCount = res.data.total
	          } else {
	            this.tableLoading = false
	            this.data1 = []
	            this.$Message.warning('暂无搜索结果,请重新搜索')
	            return false
	          }
	        })
	        .catch(err => {
	          this.tableLoading = false
	          this.$Message.error('服务器出错...')
	          console.log(err)
	        })
	    },
	
	    //trafficInfo
	    getTrafficInfo(id) {
	      this.$axios
	        .get(`${this.URL_PREFIX}/current/log/info?id=${id}`)
	        .then(res => {
	          if (res.data.code === '0') {
	            this.showTrafficInfo = true
	            const {
	              name,
	              pic,
	              device,
	              group,
	              direction,
	              type,
	              times,
	              status
	            } = res.data.result[0]
	
	            this.trafficInfo.name = name
	            this.trafficInfo.pic = JSON.parse(pic)
	            this.trafficInfo.device = device
	            this.trafficInfo.direction = direction
	            this.trafficInfo.group = group
	            this.trafficInfo.type = type
	            this.trafficInfo.times = times
	            this.trafficInfo.status = status
	          } else {
	            this.$Message.error(res.data.message)
	          }
	        })
	        .catch(err => {
	          this.$Message.error('服务器出错!')
	          console.log(err)
	        })
	    },
	    //表格数据导出
	    Export() {
	      this.export_status = true
	      this.$axios
	        .get(
	          `${this.URL_PREFIX}/current/log/export?keyword=${this.query.keyword}&direction=${this.query.direction}&times=${this.query.times}`
	        )
	        .then(res => {
	          if (res.data.code === '0') {
	            this.export_status = false
	            this.$Message.success('导出成功')
	            setTimeout(() => {
	              window.location.href = `${res.data.url}`
	            }, 500)
	          } else {
	            this.export_status = false
	            this.$Message.error('导出失败')
	          }
	        })
	        .catch(err => {
	          this.$Message.error('服务器出错!')
	          this.export_status = false
	          console.log(err)
	        })
	    },
	
	    //分页处理
	    changePage(index) {
	      this.query.currentPage = index
	      this.getTrafficList()
	    },
	
	    //获取日期选择器的起始时间
	    handleTimeChange(datetime) {
	      this.query.times = datetime
	    }
	  }
	}
	
