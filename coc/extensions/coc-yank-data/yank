22ee23654953b7309ad9bd3a34a9d086|file:///Users/aA/Desktop/chibi/cb/src/views/visitors/components/realtime.vue|319|1|V|vue
	      //打开
	      websocketopen(){
	        console.log("WebSocket连接成功")
	      },
	
	      //数据发送
	      websocketsend(agentData){
	         this.websock.send(agentData);
	     },
	
	      //数据接收
	     websocketonmessage(e){
	       let res = JSON.parse(e.data);
	       let result = res.result;
	         
	       if(res.history === 0 && result.length > 0) {
	           this.fetchUpstream(result[0].ip).then(stream => {
	               //如果和当前流是同一个流 不替换 否则进行替换更新
	               if (this.playerOptionsA.sources[0].src === stream) {
	                   return 
	               }
	
	               this.playerOptionsA.sources[0].src = stream
	               console.log(this.camsList);
	           })
	       }
	
	       if(result.length > 0){
	           result.forEach(item => {
	             let realTimeItem ={}
	             realTimeItem.id = item.id.toString();
	             realTimeItem.visitId = item.camid;
	             realTimeItem.posttime = item.posttime;
	             realTimeItem.logId = item.id.toString();
	             realTimeItem.name = item.name || '等待登记';
	             realTimeItem.pics = item.pic;
	             realTimeItem.posttime = item.posttime;
	             realTimeItem.sex = item.sex;
	             realTimeItem.age = item.age;
	
	             this.realTimeList = [...this.realTimeList,realTimeItem];
	           })
	
	         //按时间进行排序
	         this.realTimeList.sort((a,b) => {
	           return Date.parse(b.posttime) - Date.parse(a.posttime);
	         })
	
	         this.websocketsend('start');
	       }else{
	         this.websocketsend('start');
	       }
	    },
	    
	    //关闭
	      websocketclose(e){
	        console.log('WebSocket关闭');
	      },
	
	      //失败
	      websocketerror(){
	        console.log('WebSocket连接失败');
	      }
	    },
	    
	    beforeDestroy(){
	      //离开页面时关闭websocket连接
	      this.websocketsend('close');
	    }
