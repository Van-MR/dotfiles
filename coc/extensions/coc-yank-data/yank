	       //获取考勤信息次数统计
	       getAttendanceInfo(typeID){
	             this.$axios.get(`${this.URL_PREFIX}/v1/attendance/day/a?type=${typeID}`)
	                        .then(res => {
	                          if(res.data.code == '0'){
	                            this.attendanceInfo.normal = res.data.result[0].normal;   //正常
	                            this.attendanceInfo.late = res.data.result[0].late;      //迟到
	                            this.attendanceInfo.exit = res.data.result[0].exit;      //缺勤
	                            this.attendanceInfo.extence = res.data.result[0].extence;  //脱岗
	                            this.attendanceInfo.early = res.data.result[0].early;    //早退
	                          }
	                        })
	                        .catch(err => {
	                            console.log(err);
	                        })
	       },
	
