f282c35cc4e8d75fbc6d1d5cd13495a5|file:///Users/aA/Desktop/epidemic-prevention/src/libs/getGeo.js|1|1|V|javascript
	function getLocation(callback) {
	  //判断是否支持 获取本地位置
	  if (navigator.geolocation) {
	    var n = navigator.geolocation.getCurrentPosition(callback, function(fail) {
	      callback({
	        msg: '来自非https域名请求',
	        code: -2
	      });
	    });
	  } else {
	    callback({
	      msg: '你的手机不支持定位',
	      code: -1
	    });
	  }
	}
	
	// 腾讯地图定位转换坐标
	getLocation(success => {
	  // 浏览器不支持定位
	  if (success.code) {
	    layer.close(loading);
	    layer.open({
	      content: '您的手机似乎不允许定位，请检查后重试'
	    })
	  } else {
	    var lat = success.coords.latitude;
	    var lng = success.coords.longitude;
	    // //调用地图命名空间中的转换接口   type的可选值为 1:GPS经纬度，2:搜狗经纬度，3:百度经纬度，4:mapbar经纬度，5:google经纬度，6:搜狗墨卡托
	    qq.maps.convertor.translate(new qq.maps.LatLng(lat, lng), 1, function(res) {
	      //取出经纬度并且赋值
	      latlng = res[0];
	      latitude = latlng.latitude;
	      longitude = latlng.longitude;
	    })
	  }
	});
