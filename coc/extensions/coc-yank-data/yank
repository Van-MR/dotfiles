	<template>
	  <div class="home">
	    <div class="bread">
	      <p>
	        <Icon type="pie-graph" size="18" class="icon-bread"></Icon>
	        视频标签 /
	        {{ $route.meta.title }}
	      </p>
	    </div>
	    <div class="content_wrapper">
	      <div class="grid_a shadow">
	        <div class="head">
	          <h5 class="title-head">事件统计</h5>
	        </div>
	        <div class="content">
	          <div class="content_item" v-for="(item,index) in statistics" :key="index">
	            <CountTo
	              :endVal="item.value"
	              :countStyle="index % 2 === 0 ? countStyle2 : index % 3 === 0 ? countStyle3 : countStyle1 "
	            ></CountTo>
	            <p>{{item.name}}</p>
	          </div>
	        </div>
	      </div>
	      <div class="grid_b shadow">
	        <div class="head">
	          <h5 class="title-head">事件占比</h5>
	        </div>
	        <div>
	          <eventPie :chartData="pieChartData" />
	        </div>
	      </div>
	      <div class="grid_c shadow">
	        <div class="head">
	          <h5 class="title-head">事件抓拍</h5>
	        </div>
	        <div class="catch_pics">
	          <p v-for="(item,index) in pics" :key="index">
	            <img :src="item.pic" alt="catch_pic" />
	            <span>{{ item.number }}</span>
	          </p>
	        </div>
	        <div v-show="totalCount > 16">
	          <Page
	            :style="{float:'right',margin:'20px 0'}"
	            :total="totalCount"
	            size="small"
	            :page-size="pageSize"
	            show-total
	            show-elevator
	            @on-change="changepage"
	          ></Page>
	        </div>
	      </div>
	    </div>
	  </div>
	</template>
	
	<script>
	import "../home/index.scss";
	import CountTo from "../my-components/count-to/CountTo.vue";
	import eventPie from "./components/pie.vue";
	export default {
	  name: "HomeComponent",
	  components: { CountTo, eventPie },
	  data() {
	    return {
	      video_id: null,
	      pageSize: 16,
	      currentPage: 1,
	      totalCount: 0,
	      statistics: [],
	      countStyle1: {
	        fontSize: "35px",
	        color: "#92f121"
	      },
	      countStyle2: {
	        fontSize: "35px",
	        color: "#fde633"
	      },
	      countStyle3: {
	        fontSize: "35px",
	        color: "#f49731"
	      },
	      pics: [],
	      //通行统计饼图
	      pieChartData: []
	    };
	  },
	  methods: {
	    getAnalysInfo(id) {
	      this.$axios
	        .get(
	          `${this.URL_PREFIX}/device/video/file/statistic?video_id=${this.video_id}`
	        )
	        .then(res => {
	          if (res.data.code === "0") {
	            const { event, rate } = res.data.result[0];
	            this.statistics = event;
	            this.pieChartData = rate;
	          }
	        })
	        .catch(err => {
	          console.log(err);
	        });
	    },
	    getAnalyPics(id) {
	      this.$axios
	        .get(
	          `${this.URL_PREFIX}/device/video/file/statistic/pic?video_id=${this.video_id}&page=${this.currentPage}&size=${this.pageSize}`
	        )
	        .then(res => {
	          if (res.data.code === "0") {
	            this.pics = res.data.result;
	            this.totalCount = res.data.total;
	          }
	        })
	        .catch(err => {
	          console.log(err);
	        });
	    },
	    changepage(index) {
	      this.currentPage = index;
	      this.getAnalyPics();
	    }
	  },
	  mounted() {
	    this.video_id = this.$route.params.id;
	    this.getAnalysInfo(this.video_id);
	    this.getAnalyPics(this.video_id);
	  }
	};
	</script>
