	    getAttenceInfo(id) {
	      this.$axios
	        .get(`${this.URL_PREFIX}/statistic/attend/info?id=${id}`)
	        .then(res => {
	          if (res.data.code === "0") {
	            console.log(res.data.result);
	            const { id, name, attend, status, legal } = res.data.result[0];
	            this.settingConfigs.id = id;
	            this.settingConfigs.name = name;
	            this.settingConfigs.legal = legal === "1" ? true : false;
	            this.settingConfigs.shifts = attend;
	            this.settingMode = "edit";
	            this.showAddComponent = true;
	          }
	        })
	        .catch(err => {
	          this.$Message.error("服务器出错!");
	          console.log(err);
	        });
	    },
