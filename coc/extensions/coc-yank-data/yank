cbd66630357f14b473b230dede2f7e68|file:///Users/aA/Desktop/chibi/cb/src/views/statistics/charts/line1.vue|60|12|v|vue
	{
	    setOptions({ xData, expectedData, legendData } = {}) {
	      this.chart = echarts.init(this.$el, "macarons");
	      let series = legendData.map((item, index) => {
	        return {
	          name: item,
	          type: "line",
	          data: expectedData[index],
	          smooth: false
	        };
	      });
	      this.chart.setOption({
	        color: ["#D53A35", "#E98F6F", "#6AB0B8", "#334B5C"],
	        tooltip: {
	          trigger: "axis"
	        },
	        legend: {
	          data: legendData
	        },
	        grid: {
	          left: "3%",
	          right: "4%",
	          bottom: "3%",
	          containLabel: true
	        },
	        xAxis: {
	          type: "category",
	          boundaryGap: false,
	          data: xData
	        },
	        yAxis: {
	          type: "value",
	          name: "单位:(次)"
	        },
	        series: series
	      });
	    },
	    initChart() {
	      this.chart = echarts.init(this.$el, "macarons");
	      this.setOptions(this.chartData);
	      var _this = this;
	      window.onresize = function() {
	        _this.chart.resize();
	      };
	      window.onload = function() {
	        _this.chart.resize();
	      };
	    }
	  }
