	  //获取uri重定向地址中的code
	  const getParams = () => {
	    let code;
	    let location = window.location.href;
	    let reg = /\&code=/;
	    let result = reg.test(location);
	    if (result) {
	      let k = location.split("?")[1];
	      let d = k.split("&")[1];
	      let f = d.substr(0, 5);
	      code = d.replace(f, "");
	      return code;
	    }
	
	    code = "";
	    return code;
	  };
	
