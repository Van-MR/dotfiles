	        let res = await fetch(`${baseURL}/common/upload/pic`, {
	            method: 'POST',
	            mode: 'no-cors',
	            headers: {
	                'Content-Type': 'multipart/form-data'
	            },
	            body: formData
	        })
	
	        let jsonData = await res.json()
	
	        if (jsonData.code === '0') {
	            this.setState({
	                toastText: '更新头像成功',
	                toastType: 'success',
	                showToast: true
	            })
	            setTimeout(() => {
	                this.setState({
	                    showToast: false,
	                    file: '',
	                    data: {
	                        ...this.state.data,
	                        pic: jsonData.url
	                    },
	                })
	            }, 1000)
	        } else {
	            this.setState({
	                toastText: '更新头像失败',
	                toastType: 'error',
	                showToast: true
	            })
