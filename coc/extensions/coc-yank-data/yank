	    //文件类型验证
	    checkFileType(file) {
	      const fileExt = file.name.split(".")[1];
	      const AllowEtxs = ["xls", "xml", "csv"];
	      const isAllow = AllowEtxs.includes(fileExt);
	      if (!isAllow) {
	        this.file = "";
	        this.showUploadModal = false;
	        this.$Message.error("请选择合适的文件类型进行上传,如xxx.xls");
	      }
	    },
