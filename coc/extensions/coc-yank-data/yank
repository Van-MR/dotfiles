29e9a12c380b1c8795fcef19944dd973|file:///Users/aA/Desktop/chibi/cb/src/views/traffic/components/trafficRecord.vue|172|1|V|vue
	<script>
	import { mapState, mapActions } from "vuex";
	import CountTo from "../../my-components/count-to/CountTo.vue";
	export default {
	  name: "traffic_event",
	  components: { CountTo },
	  data() {
	    return {
	      tableLoading: false,
	      searchLoading: false,
	      export_status: false,
	      showTrafficInfo: false,
	      directions: [
	        { id: 0, name: "进门" },
	        { id: 1, name: "出门" }
	      ],
	      infos: [],
	      trafficInfo: {},
	        colorsArr:[
	
	        ],
	      countStyle1: {
	        fontSize: "25px",
	        color: "#92f121"
	      },
	      countStyle2: {
	        fontSize: "25px",
	        color: "#fde633"
	      },
	      countStyle3: {
	        fontSize: "25px",
	        color: "#f49731"
	      },
	      countStyle4: {
	        fontSize: "25px",
	        color: "#ed4635"
	      },
	      countStyle5: {
	        fontSize: "25px",
	        color: "#2a97f3"
	      },
	      query: {
	        keyword: "",
	        direction: "",
	        times: "",
	        currentPage: 1,
	        pageSize: 8
	      },
	      totalCount: 0,
	      data1: [],
	      columns1: [
	        {
	          title: "序号",
	          key: "id",
	          width: 80,
	          align: "center"
	        },
	        {
	          title: "照片",
	          key: "pic",
	          width: 100,
	          align: "center",
	          render: (h, params) => {
	            let src = params.row.pic;
	            return h("div", [
	              h("img", {
	                attrs: {
	                  src
	                },
	                style: {
	                  width: "45px",
	                  height: "45px",
	                  cursor: "pointer",
	                  marginTop: "5px"
	                }
	              })
	            ]);
	          }
	        },
	        {
	          title: "设备名称",
	          key: "device",
	          align: "center"
	        },
	        {
	          title: "进出方向",
	          key: "direction",
	          align: "center"
	        },
	        {
	          title: "通行时间",
	          key: "times",
	          align: "center"
	        },
	        {
	          title: "操作",
	          align: "center",
	          render: (h, params) => {
	            return h("div", [
	              h(
	                "p",
	                {
	                  style: {
	                    color: "#258ee9",
	                    cursor: "pointer"
	                  },
	                  on: {
	                    click: () => {
	                      this.getTrafficInfo(params.row.id);
	                    }
	                  }
	                },
	                "查看详情"
	              )
	            ]);
	          }
	        }
	      ],
	      data1: []
	    };
	  },
	  mounted() {
	    this.getStasticInfo();
	    this.getTrafficList();
	  },
	  methods: {
	    //获取人流出行记录信息
	    getStasticInfo(times = "") {
	      this.$axios
	        .get(`${this.URL_PREFIX}/current/log/statistic?times=`)
	        .then(res => {
	          if (res.data.code === "0") {
	            this.infos = res.data.result;
	          }
	        })
	        .catch(err => {
	          console.log(err);
	        });
	    },
	    //获取通行记录列表
	    getTrafficList() {
	      this.tableLoading = true;
	      this.$axios
	        .get(
	          `${this.URL_PREFIX}/current/log?page=${this.query.currentPage}&size=${
	            this.query.pageSize
	          }&keyword=${this.query.keyword || ""}&direction=${this.query
	            .direction || ""}&times=${this.query.times || ""}`
	        )
	        .then(res => {
	          if (res.data.code == "0") {
	            this.tableLoading = false;
	            this.data1 = res.data.result;
	            this.totalCount = res.data.total;
	          } else {
	            this.tableLoading = false;
	            this.data1 = [];
	            this.$Message.warning("暂无搜索结果,请重新搜索");
	            return false;
	          }
	        })
	        .catch(err => {
	          this.tableLoading = false;
	          this.$Message.error("服务器出错...");
	          console.log(err);
	        });
	    },
	
	    //trafficInfo
	    getTrafficInfo(id) {
	      this.$axios
	        .get(`${this.URL_PREFIX}/current/log/info?id=${id}`)
	        .then(res => {
	          if (res.data.code === "0") {
	            this.showTrafficInfo = true;
	            const {
	              name,
	              pic,
	              device,
	              group,
	              direction,
	              type,
	              times,
	              status
	            } = res.data.result[0];
	
	            this.trafficInfo.name = name;
	            this.trafficInfo.pic = JSON.parse(pic);
	            this.trafficInfo.device = device;
	            this.trafficInfo.direction = direction;
	            this.trafficInfo.group = group;
	            this.trafficInfo.type = type;
	            this.trafficInfo.times = times;
	            this.trafficInfo.status = status;
	          } else {
	            this.$Message.error(res.data.message);
	          }
	        })
	        .catch(err => {
	          this.$Message.error("服务器出错!");
	          console.log(err);
	        });
	    },
	    //表格数据导出
	    Export() {
	      this.export_status = true;
	      this.$axios
	        .get(
	          `${this.URL_PREFIX}/current/log/export?keyword=${this.query.keyword}&direction=${this.query.direction}&times=${this.query.times}`
	        )
	        .then(res => {
	          if (res.data.code === "0") {
	            this.export_status = false;
	            this.$Message.success("导出成功");
	            setTimeout(() => {
	              window.location.href = `${res.data.url}`;
	            }, 500);
	          } else {
	            this.export_status = false;
	            this.$Message.error("导出失败");
	          }
	        })
	        .catch(err => {
	          this.$Message.error("服务器出错!");
	          this.export_status = false;
	          console.log(err);
	        });
	    },
	
	    //分页处理
	    changePage(index) {
	      this.query.currentPage = index;
	      this.getTrafficList();
	    },
	
	    //获取日期选择器的起始时间
	    handleTimeChange(datetime) {
	      this.query.times = datetime;
	    }
	  }
	};
	</script>
