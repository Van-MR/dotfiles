	TextGeometry
bc16bbd07c8575eb4f647df780c5e338|file:///Users/aA/Desktop/webGl/samples/axis.html|21|5|V|html
	    <script>
	      let scene,
	        camera,
	        renderer,
	        planeGeometry,
	        planeMaterial,
	        plane,
	        cubeGeoMetry,
	        cubeMaterial,
	        cube,
	        textGeometry,
	        textMaterial,
	        text,
	        axis,
	        controls,
	        datGUI,
	        guiControls,
	        stats,
	        SCREEN_WIDTH,
	        SCREN_HEIGHT;
	
	      function init() {
	        scene = new THREE.Scene();
	
	        renderer = new THREE.WebGLRenderer({ antialias: true });
	        renderer.setSize(window.innerWidth, window.innerHeight);
	        renderer.setClearColor(0x000000);
	        document.body.appendChild(renderer.domElement);
	
	        camera = new THREE.PerspectiveCamera(
	          45,
	          window.innerWidth / window.innerHeight,
	          1,
	          500
	        );
	        camera.position.set(70, 40, 50);
	        camera.lookAt(0, 0, 0);
	
	        controls = new THREE.OrbitControls(camera, renderer.domElement);
	        controls.addEventListener("change", renderer);
	
	        //axis
	        axis = new THREE.AxesHelper(15);
	        scene.add(axis);
	
	        //grid
	        //var color = new THREE.Color("teal");
	        var grid = new THREE.GridHelper(50, 25);
	
	        scene.add(grid);
	
	        //plane
	        var planeGeometry = new THREE.PlaneGeometry(30, 30, 30);
	        var planeMaterial = new THREE.MeshLambertMaterial({
	          color: 0xffffff,
	          side: THREE.DoubleSide
	        });
	        var plane = new THREE.Mesh(planeGeometry, planeMaterial);
	        plane.receiveShadow = true;
	        plane.rotation.x = 0.5 * Math.PI;
	
	        scene.add(plane);
	
	        plane.rotation.x = 0.5 * Math.PI;
	        plane.receiveShadow = true;
	
	        scene.add(plane);
	
	        //cube
	        cubeGeoMetry = new THREE.BoxGeometry(5, 5, 5);
	        cubeMaterial = new THREE.MeshLambertMaterial({ color: "red" });
	        cube = new THREE.Mesh(cubeGeoMetry, cubeMaterial);
	
	        cube.position.set(2.5, 2.5, 2.5);
	        scene.add(cube);
	
	        //text
	        textGeometry = new THREE.TextGeometry("hello world", [
	          size: 2,
	          height: 1
	        ]);
	        textMaterial = new THREE.MeshPhongMaterial({ color: "green" });
	        text = new THREE.Mesh(textGeometry, textMaterial);
	
	        // spot light
	        var spotLight = new THREE.SpotLight("#ececec", 0.9);
	        spotLight.position.set(100, 1000, 100);
	        spotLight.castShadow = true;
	
	        spotLight.shadow.mapSize.width = 1024;
	        spotLight.shadow.mapSize.height = 1024;
	
	        spotLight.shadow.camera.near = 500;
	        spotLight.shadow.camera.far = 4000;
	        spotLight.shadow.camera.fov = 30;
	
	        scene.add(spotLight);
	
	        //dat.gui  inittial property value
	        guiControls = new (function() {
	          this.rotationX = 0;
	          this.rotationY = 0.01;
	          this.rotationZ = 0;
	        })();
	
	        datGUI = new dat.GUI();
	        //setting range  min and max value
	        datGUI.add(guiControls, "rotationX", 0, 1);
	        datGUI.add(guiControls, "rotationY", 0, 1);
	        datGUI.add(guiControls, "rotationZ", 0, 1);
	
	        animate();
	      }
	
	      function animate() {
	        cube.rotation.x += guiControls.rotationX;
	        cube.rotation.y += guiControls.rotationY;
	        cube.rotation.z += guiControls.rotationZ;
	
	        renderer.render(scene, camera);
	        requestAnimationFrame(animate);
	      }
	
	      init();
	    </script>
