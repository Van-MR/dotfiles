	                method: 'POST',
	                body: formData
	            })
	
	            let jsonData = await res.json()
	
	            if (jsonData.code !== '0') {
	                this.setState({
	                    toastText: jsonData.msg,
	                    toastType: 'error',
	                    file: '',
	                    showToast: true
	                })
	                setTimeout(() => {
	                    this.setState({
	                        showToast: false
	                    })
	                }, 1000)
	
	                return;
	            }
	
