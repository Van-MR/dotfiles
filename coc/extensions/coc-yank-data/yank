	{
	      this.$axios
	        .get(
	          `${this.URL_PREFIX}/person/list?page=${this.currentPage}&size=${
	            this.pageSize
	          }&keyword=${this.keyword}&sex=${this.sex || ""}&times=${this.times}`
	        )
	        .then(res => {
	          if (res.data.code === "0") {
	            this.tableLoading = false;
	            this.data = res.data.result;
	            this.totalCount = res.data.total;
	          }
	        })
	        .catch(err => {
	          console.log(err);
	          this.tableLoading = false;
	        });
	    }
