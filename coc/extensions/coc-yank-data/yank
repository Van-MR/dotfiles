329f14b2fce1163ed59814aefcdf7481|file:///Users/aA/Desktop/epidemicPrevention/src/views/degencense/list.vue|182|1|V|vue
	    ...mapActions(["getGroupList"]),
	    //设备厂家词典
	    getFactoryDic() {
	      this.$axios
	        .get(`${this.URL_PREFIX}/common/dict/factory`)
	        .then(res => {
	          if (res.data.code === "0") {
	            this.factoryList = res.data.result;
	          }
	        })
	        .catch(err => {
	          console.log(err);
	        });
	    },
	    //获取设备列表
	    getDiviceList() {
	      this.$axios
	        .get(
	          `${this.URL_PREFIX}/device/list?page=${this.currentPage}&size=${
	            this.pageSize
	          }&keyword=${this.keyword}&group=${this.group || ""}&status=${this
	            .status || ""}&times=${this.times}`
	        )
	        .then(res => {
	          if (res.data.code === "0") {
	            this.tableLoading = false;
	            this.data = res.data.result;
	            this.totalCount = res.data.total;
	          }
	        })
	        .catch(err => {
	          console.log(err);
	          this.tableLoading = false;
	        });
	    },
	    //搜索
	    Search() {
	      this.tableLoading = true;
	      this.getDiviceList();
	    },
	    //模板文件下载
	    downloadTemplate() {
	      window.location.href = `${this.Download_URL}/mould/设备导入模板.xls`;
	    },
	    //导出
	    Export() {
	      this.$axios
	        .get(
	          `${this.URL_PREFIX}/device/export?page=${this.currentPage}&size=${
	            this.pageSize
	          }&keyword=${this.keyword}&status=${this.status || ""}&group=${this
	            .group || ""}`
	        )
	        .then(res => {
	          if (res.data.code === "0") {
	            window.location.href = `${res.data.url}`;
	          }
	        })
	        .catch(err => {
	          console.log(err);
	        });
	    },
	    //导入文件类型验证
	    checkFileType(file) {
	      const fileExt = file.name.split(".")[1];
	      const AllowEtxs = ["xls"];
	      const isAllow = AllowEtxs.includes(fileExt);
	      if (!isAllow) {
	        this.file = "";
	        this.showUploadModal = false;
	        this.$Message.error("请选择合适的文件类型进行上传,如xxx.xls");
	      }
	    },
	    //文件导入钩子
	    handleBeforeUpload1(file) {
	      this.file = file;
	      this.checkFileType(this.file);
	    },
	    //文件导入
	    Import() {
	      let fForm = new FormData();
	      fForm.append("files", this.file);
	      let config = {
	        headers: {
	          "Content-Type": "multipart/form-data"
	        }
	      };
	      this.$axios
	        .post(`${this.URL_PREFIX}/device/export`, fForm, config)
	        .then(res => {
	          if (res.data.code === "0") {
	            this.showUploadModal = false;
	            this.file = "";
	            this.$Message.success("导入成功");
	            this.getDiviceList();
	          }
	        })
	        .catch(err => {
	          this.showUploadModal = false;
	          this.file = "";
	          console.log(err);
	          this.$Message.error("服务器出错...");
	        });
	    },
	    //添加设备
	    AddDevice() {
	      this.$refs["deviceForm"].validate(valid => {
	        if (valid) {
	          this.equipAddForm.port = this.equipAddForm.port + "";
	          this.$axios
	            .post(`${this.URL_PREFIX}/device/device`, this.equipAddForm)
	            .then(res => {
	              if (res.data.code == "0") {
	                this.getDiviceList();
	                this.showAddModal = false;
	                this.$Message.success("添加成功");
	                this.$refs["deviceForm"].resetFields();
	              } else if (res.data.code == "-10011") {
	                this.$Message.error(`${res.data.msg}`);
	              }
	            })
	            .catch(err => {
	              console.log(err);
	              this.showAddModal = false;
	              this.$Message.error("添加失败");
	            });
	        }
	      });
	    },
	    //获取设备详情
	    getDeviceInfo(id) {
	      this.$axios
	        .get(`${this.URL_PREFIX}/device/device?id=${id}`)
	        .then(res => {
	          this.showCompileModal = true;
	          this.equipCompileForm = res.data.result[0];
	        })
	        .catch(err => {
	          console.log(err);
	          this.$Message.error("服务器出错。。。");
	        });
	    },
	    //设备信息修改
	    updateDevice(id) {
	      this.equipCompileForm.port = this.equipCompileForm.port + "";
	      this.$axios
	        .put(`${this.URL_PREFIX}/device/device`, this.equipCompileForm)
	        .then(res => {
	          if (res.data.code === "0") {
	            this.$Message.success("编辑成功");
	            this.showCompileModal = false;
	            this.getDiviceList();
	          }
	        })
	        .catch(err => {
	          console.log(err);
	          this.$Message.error("服务器出错。。。");
	        });
	    },
	    //设备删除
	    deleteDevice(id) {
	      this.$axios
	        .delete(`${this.URL_PREFIX}/device/device`, {
	          data: { id }
	        })
	        .then(res => {
	          if (res.data.code === "0") {
	            this.$Message.success("删除成功");
	            this.getDiviceList();
	          }
	        })
	        .catch(err => {
	          console.log(err);
	          this.$Message.error("服务器出错。。。");
	        });
	    },
	    //分页
	    changePage(page) {
	      this.currentPage = page;
	      this.getDiviceList();
	    },
	    //datePicker change event
	    handleTimeChange(datetime) {
	      this.times = datetime;
	    }
