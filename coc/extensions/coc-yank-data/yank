254376ae57c7cf330906f1a1b6df67d1|file:///Users/aA/Desktop/chibi/cb/src/views/visitors/components/realtime.vue|273|1|V|vue
	    //访客登记
	    visitorRegister() {
	      this.$refs["register"].validate(valid => {
	        if (valid) {
	          this.$axios
	            .post(`${this.URL_PREFIX}/monitor`, this.userRegisterForm)
	            .then(res => {
	              if (res.data.code === "0") {
	                this.showVisitorModal = false;
	                this.$Message.success("登记成功");
	                this.realTimeList.forEach(item => {
	                  if (item.logId === this.userRegisterForm.id) {
	                    item.name = this.userRegisterForm.username;
	                    this.$refs["register"].resetFields();
	                  }
	                });
	              }
	            })
	            .catch(err => {
	              this.showVisitorModal = false;
	              this.$refs["register"].resetFields();
	              this.$Message.error("服务器出错");
	              console.log(err);
	            });
	        }
	      });
	    },
