	    //切换不同失败场景 人脸 抽烟 安全帽
	    changeAlgorithmType(type) {
	      this.type = type
	      this.loadAlgorithm()
	    },
	
	    //加载算法
	    loadAlgorithm(ip) {
	      this.$axios
	        .post(`${this.URL_PREFIX}/device/video/label`, {
	          ip: this.currentIP,
	          type: this.type
	        })
	        .then(res => {
	          if (res.data.code === '0') {
	            this.$Message.success(res.data.msg)
	          } else {
	            this.$Message.error(res.data.msg)
	          }
	        })
	        .catch(err => {
	          this.$Message.error('服务器出出错...')
	          console.log(err)
	        })
	    },
	
	    //视频推流
	    fetchUpstream(ip) {
	      return new Promise((resolve, reject) => {
	        this.$axios
	          .post(`${this.URL_PREFIX}/device/video/rtmp`, {
	            ip
	          })
	          .then(res => {
	            if (res.data.code === '0') {
	              resolve(res.data.url)
	            }
	          })
	          .catch(err => {
	            reject(err)
	            this.$Message.error('服务器出错')
	          })
	      })
	    },
	
	    //获取频道列表
	    getChanels() {
	      this.$axios
	        .get(`${this.URL_PREFIX}/device/video/list?keyword=`)
	        .then(res => {
	          if (res.data.code === '0') {
	            this.channelList = res.data.result
	            this.currentIP = this.channelList[0].ip
	            this.loadAlgorithm(this.currentIP)
	            this.fetchUpstream(this.currentIP).then(url => {
	              this.playerOptions.sources[0].src = url
	            })
	          }
	        })
	        .catch(err => {
	          console.log(err)
	        })
	    },
	
	    //切换通道
	    switchCam(event, item, index) {
	      this.currentIP = item.ip
	      this.currentIndex = index
	      this.loadAlgorithm(this.currentIP)
	      this.fetchUpstream(this.currentIP).then(url => {
	        this.playerOptions.sources[0].src = url
	      })
	    },
	
	    initWebSocket() {
	      const wsurl = `${this.URL_WS1}`
	      this.websock = new WebSocket(wsurl)
	      this.websock.onopen = this.websocketopen
	      this.websock.onmessage = this.websocketonmessage
	      this.websock.onclose = this.websocketclose
	    },
	
	    beforeunloadHandler(e) {
	      this.websock.send('close')
	    },
	
	    loadTimer() {
	      this.timer = setInterval(() => {
	        this.countTime++
	      }, 1000)
	    },
	
	    closeTimer() {
	      clearInterval(this.timer)
	      this.countTime = 0
	    },
	
	    //打开
	    websocketopen() {
	      this.websock.send('start')
	    },
	
	    //数据发送
	    websocketsend() {
	      //this.websock.send("start");
	    },
	
	    websocketclose() {
	      //this.websock.send("close");
	    },
	
	    //数据接收
	    websocketonmessage(e) {
	      let res = JSON.parse(e.data)
	      let result = res.result
	      this.websock.send('start')
	      if (result.length > 0 && !res.history) {
	        this.shouldReforceUpdate = false
	        this.closeTimer()
	        //对象destructor 赋值
	        let {
	          name,
	          age,
	          type,
	          sex,
	          posttime,
	          group,
	          pic,
	          hal_status,
	          smoke_status
	        } = res.result[0]
	
	        //当前返回的该条记录
	        let logItem = {
	          name,
	          age,
	          sex,
	          posttime,
	          group,
	          type,
	          pic: pic[0],
	          hal_status,
	          smoke_status
	        }
	
	        //查找当前最新返回的记录是否存在于记录集当中
	        let exitIndex = this.faceLogs.findIndex(
	          log => log.name === logItem.name
	        )
	
	        //如果最新返回的记录之前存在,就替换掉之前同名的那条记录
	        if (exitIndex > -1) {
	          this.faceLogs[exitIndex] = logItem
	          this.shouldReforceUpdate = true
	          this.loadTimer()
	        } else {
	          //如果不存在,就往记录集当中添加,同时当卡片数超过3个时,新的记录按当前卡片排序依次进行替换
	          this.offsetIndex++
	          if (this.offsetIndex < 4) {
	            this.faceLogs = [...this.faceLogs, logItem]
	            this.shouldReforceUpdate = true
	          } else {
	            let remain_Index = this.offsetIndex % 3
	            remain_Index = remain_Index === 0 ? 3 : remain_Index
	            this.faceLogs[remain_Index - 1] = logItem
	            this.shouldReforceUpdate = true
	          }
	          this.loadTimer()
	        }
	
	        //记录超出屏幕时,自动滚动展示
	        //if (this.faceLogs.length > 3) {
	        //  var target = document.getElementsByClassName("facelogs")[0];
	        //  target.style.transform = `translateY(${-240 *
	        //    (this.offsetIndex - 3)}px)`;
	        //}
	      }
	    }
