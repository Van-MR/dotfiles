	const withUserComponent = Component => {
	  const [user, setUser] = useState(null);
	
	  useEffect(() => {
	    axios
	      .get("http;//jsonplaceholder/users")
	      .then(res => {
	        setUser(res.data);
	      })
	      .catch(err => {
	        console.log(err);
	      });
	  }, []);
	
	  const newComponent = props => {
	    return <Component {...props} user={user} />;
	  };
	
	  return newComponent;
	};
