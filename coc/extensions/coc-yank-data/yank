	                        <p slot="title">考勤日分析</p>
	                        <div class="">
	                           <Pie  :chart-data="lineChartData"></Pie>
	                        </div>
	                      </Card>
	                    </Col>
	                    <Col span="12">
	                      <Card :bordered="false" :dis-hover="true":style="{height:'350px',marginLeft:'10px'}">
	                        <p slot="title">部门考勤日分析</p>
	                        <div class="">
	                            <Bar  :chartData2 = "lineChartData1" width="90%"></Bar>
	                        </div>
	                      </Card>
	                    </Col>
	                </Row>
	                <Row :style="{marginLeft:'15px',marginTop:'8px',marginBottom:'12px',backgroundColor:'#F0F2F5'}">
	                   <Col>
	                      <Card :bordered="false" :dis-hover="true" :style="{height:'300px',marginRight:'15px'}">
	                        <p slot="title">日出勤率</p>
	
	                        <div class="">
	                            <Bar1 :chartData2 = "lineChartData2"  height="220px"></Bar1>
	                        </div>
	                      </Card>
	                   </Col>
	                </Row>
	            </Col>
	            <Col span="7" :style="{position:'absolute',right:'12px',boxSizing:'borderBox'}">
	              <Card :bordered="false" :dis-hover="true" :style="{height:'655px',marginLeft:'10px'}">
	                <p slot="title">迟到员工</p>
	                <div class="list">
	                    <p class="titles">
	                      <span>员工</span>
	                      <span>部门</span>
	                      <span>时间</span>
	                      <span>状态</span>
	                    </p>
	                    <p class="content" v-for="(item,index) in this.lateList" :style="{cursor:'pointer'}">
	                      <span>{{item.name}}</span>
	                      <span>{{item.tment}}</span>
	                      <span>{{item.times}}</span>
	                      <span class="status" :class="{'active': item.status == '早退'}">{{item.status}}</span>
	                    </p>
	                </div>
	              </Card>
	            </Col>
	        </Row>
	      </div>
	      <div v-if="currentIndex == 1">
	        <Row :style="{height:'200px',backgroundColor:'#F0F2F5'}">
	           <div class="dashboard">
	                <div class="card">
	                    <p class="tit">正常</p>
	                    <p class="times">
	                        <span :style="{marginLeft:'16px',marginTop:'10px',fontSize:'30px'}">{{ this.attendanceInfo.normal[0] }}</span>
	                        <span :style="{marginRight:'10px',marginTop:'30px',fontSize:'18px'}">次</span>
	                    </p>
	                    <p class="splitline"></p>
	                    <p class="propotion">
	                      <span :style="{marginLeft:'12px'}">同比上月</span>
	                      <span class="degrese"></span>
	                      <span class="rate" :style="{marginRight:'12px'}">{{ this.attendanceInfo.normal[1] }}</span>
	                    </p>
	                </div>
	                <div class="card">
	                    <p class="tit">迟到</p>
	                    <p class="times">
	                        <span :style="{marginLeft:'16px',marginTop:'10px',fontSize:'30px'}">{{ this.attendanceInfo.late[0] }}</span>
	                        <span :style="{marginRight:'10px',marginTop:'30px',fontSize:'18px'}">次</span>
	                    </p>
	                    <p class="splitline"></p>
	                    <p class="propotion">
	                      <span :style="{marginLeft:'12px'}">同比上月</span>
	                      <span class="increse"></span>
	                      <span class="rate" :style="{marginRight:'12px'}">{{ this.attendanceInfo.normal[1] }}</span>
	                    </p>
	                </div>
	                <div class="card">
	                    <p class="tit">早退</p>
	                    <p class="times">
	                        <span :style="{marginLeft:'16px',marginTop:'10px',fontSize:'30px'}">{{ this.attendanceInfo.early[0] }}</span>
	                        <span :style="{marginRight:'10px',marginTop:'30px',fontSize:'18px'}">次</span>
	                    </p>
	                    <p class="splitline"></p>
	                    <p class="propotion">
	                      <span :style="{marginLeft:'12px'}">同比上月</span>
	                      <span class="degrese"></span>
	                      <span class="rate" :style="{marginRight:'12px'}">{{ this.attendanceInfo.early[1] }}</span>
	                    </p>
	                </div>
	                <div class="card">
	                    <p class="tit">脱岗</p>
	                    <p class="times">
	                        <span :style="{marginLeft:'16px',marginTop:'10px',fontSize:'30px'}">{{ this.attendanceInfo.extence[0] }}</span>
	                        <span :style="{marginRight:'10px',marginTop:'30px',fontSize:'18px'}">次</span>
	                    </p>
	                    <p class="splitline"></p>
	                    <p class="propotion">
	                      <span :style="{marginLeft:'12px'}">同比上月</span>
	                      <span class="degrese"></span>
	                      <span class="rate" :style="{marginRight:'12px'}">{{ this.attendanceInfo.extence[1] }}</span>
	                    </p>
	                </div>
	                <div class="card">
	                    <p class="tit">缺勤</p>
	                    <p class="times">
	                        <span :style="{marginLeft:'16px',marginTop:'10px',fontSize:'30px'}">{{ this.attendanceInfo.exit[0] }}</span>
	                        <span :style="{marginRight:'10px',marginTop:'30px',fontSize:'18px'}">次</span>
	                    </p>
	                    <p class="splitline"></p>
	                    <p class="propotion">
	                      <span :style="{marginLeft:'12px'}">同比上月</span>
	                      <span class="increse"></span>
	                      <span class="rate" :style="{marginRight:'12px'}">{{ this.attendanceInfo.exit[1] }}</span>
	                    </p>
	                </div>
	           </div>
	        </Row>
	        <Row :style="{backgroundColor:'#F0F2F5',marginTop:'-33px'}">
	
	                <Row :style="{marginLeft:'15px',marginRight:'15px',backgroundColor:'#F0F2F5',boxSizing:'borderBox'}" >
	                    <Col span="9" >
	                      <Card :bordered="false"  :dis-hover="true" :style="{height:'350px'}">
	                        <p slot="title">考勤月分析</p>
	                        <div class="">
	                          <Pie1 :chart-data="lineChartData"></Pie1>
	                        </div>
	                      </Card>
	                    </Col>
	                    <Col span="9">
	                      <Card :bordered="false" :dis-hover="true":style="{height:'350px',marginLeft:'10px'}">
	                        <p slot="title">部门考勤月分析</p>
	                        <div class="">
	                           <Bar2 :chartData2 = "lineChartData1"></Bar2>
	                        </div>
	                      </Card>
	                    </Col>
	                    <Col span="6" :style="{boxSizing:'borderBox'}">
	                      <Card :bordered="false" :dis-hover="true" :style="{height:'350px',marginLeft:'10px'}">
	                        <p slot="title">部门迟到排行</p>
	                        <div class="list_">
	                            <p class="titles">
	                              <span>排名</span>
	                              <span>部门</span>
	                              <span>次数</span>
	                            </p>
	                            <p class="content" v-for="(item,index) in this.depList">
	                              <span class="rank">{{item.rank}}</span>
	                              <span>{{item.dep}}</span>
	                              <span>{{item.times}}</span>
	                            </p>
	                        </div>
	                      </Card>
	                    </Col>
	                </Row>
	                <Row :style="{marginLeft:'15px',marginTop:'8px',marginBottom:'12px',backgroundColor:'#F0F2F5',boxSizing:'borderBox'}">
	                   <Col span="18">
	                      <Card :bordered="false" :dis-hover="true" :style="{height:'350px',marginRight:'10px'}">
	                        <p slot="title">考勤月分析曲线图</p>
	                        <div class="">
	                            <Lines :chartData2 = "lineChartData2"></Lines>
	                        </div>
	                      </Card>
	                   </Col>
	                   <Col span="6" :style="{boxSizing:'borderBox'}">
	                     <Card :bordered="false" :dis-hover="true" :style="{height:'350px',marginRight:'15px'}">
	                       <p slot="title">员工迟到排行</p>
	                       <div class="list_">
	                           <p class="titles">
	                             <span>排名</span>
	                             <span>员工</span>
	                             <span>部门</span>
	                             <span>次数</span>
	                           </p>
	                           <p class="content" v-for="(item,index) in this.emploee_list" :style="{cursor:'pointer'}" @click="goDetail($event,item)">
	                             <span class="rank">{{item.rank}}</span>
	                             <span>{{item.name}}</span>
	                             <span>{{item.tment}}</span>
	                             <span>{{item.times}}</span>
	                           </p>
	                       </div>
	                     </Card>
	                   </Col>
	                </Row>
	
	
	        </Row>
	
	      </div>
	
	<!-- 每日 每月  tab栏切换 -->
	      <div class="tablist" :style="{marginBottom:'30px'}">
	         <ul>
	            <li v-for="(item,index) in  tabList" :class="{'active':currentIndex === index}"   @click="changeTab(index)">{{item}}</li>
	         </ul>
	      </div>
	
	    </div>
	</template>
	
	<script>
	
	import Pie from './components/pie.vue'
	import Pie1 from './components/pie1.vue'
	import Bar  from './components/bar.vue'
	import Bar1  from './components/bar1.vue'
	import Bar2  from './components/bar2.vue'
	import Lines from './components/lines.vue'
	
	export default {
	     data(){
	       return {
	          typeID:'1',
	          msg:'考勤分析',
	          currentIndex:0,
	          attendanceInfo:{
	            normal:0,
	            late:0,
	            early:0,
	            extence:0,
	            exit:0
	          },
	          lineChartData: {
	           xData:[],
	           expectedData: []
	          },
	          lineChartData1: {
	           xData:[],
	           expectedData: []
	          },
	          lineChartData2: {
	           xData:[],
	           expectedData: []
	          },
	          pieData:[],
	          tabList:['每天','每月'],
	          lateList:[],
	          depList:[],
	          emploee_list:[]
	       }
	     },
	     components:{
	        Pie,
	        Pie1,
	        Bar,
	        Bar1,
	        Bar2,
	        Lines
	     },
	     watch:{
	
	     },
	     created(){
	         this.getAll(this.typeID);
	     },
	     mounted(){
	          this.getAttendanceInfo(this.typeID);
	          this.getEmploeeList(this.typeID);
	
	     },
	     methods:{
	
	       /*
	       * @params  typeID    1代表日分析   2代表月分析
	       */
	       //获取考勤信息次数统计
	       getAttendanceInfo(typeID){
	             this.$axios.get(`${this.URL_PREFIX}/v1/attendance/day/a?type=${typeID}`)
	                        .then(res => {
	                          if(res.data.code == '0'){
	                            this.attendanceInfo.normal = res.data.result[0].normal;   //正常
	                            this.attendanceInfo.late = res.data.result[0].late;      //迟到
	                            this.attendanceInfo.exit = res.data.result[0].exit;      //缺勤
	                            this.attendanceInfo.extence = res.data.result[0].extence;  //脱岗
	                            this.attendanceInfo.early = res.data.result[0].early;    //早退
	                          }
	                        })
	                        .catch(err => {
	                            console.log(err);
	                        })
	       },
	       //并发请求 所有图表数据
	       getAll(typeID){
	            this.$axios.all([this.getPie(typeID),this.getDepBars(typeID),this.getRateBars(typeID)])
	                       .then(this.$axios.spread((res1,res2,res3) => {
	                            //饼图data
	                            this.lineChartData.expectedData = res1.data.result;
	                            //多组柱状图 data
	                            this.lineChartData1.xData = ['研发部', '商务部', '市场部', '投资部', '财务部'];
	                            this.lineChartData1.expectedData = res2.data.result[1];
	
	                            //日出勤率data
	                            this.lineChartData2.xData = res3.data.result[0];
	                            this.lineChartData2.expectedData = res3.data.result[1];
	
	                       }))
	       },
	       //考勤日分析 饼图
	       getPie(typeID){
	           return this.$axios.get(`${this.URL_PREFIX}/v1/attendance/day/b?type=${typeID}`);
	
	       },
	       //部门考勤日分析  多组柱状图
	       getDepBars(typeID){
	           return this.$axios.get(`${this.URL_PREFIX}/v1/attendance/day/c?type=${typeID}`);
	       },
	       //部门日出勤率  单租柱状图
	       getRateBars(typeID){
	          return this.$axios.get(`${this.URL_PREFIX}/v1/attendance/day/d?type=${typeID}`);
	       },
	       //切换tab栏  切换日分析和月分析页面
	       changeTab(index){
	         this.currentIndex = index;
	         this.typeID = this.currentIndex+1;
	         this.getAttendanceInfo(this.typeID);
	
	         //部门考勤排行
	         this.getEmploeeList(this.typeID);
	
	
	         if(this.typeID === 2){
	           //月考勤员工 排行表
	           this.$axios.get(`${this.URL_PREFIX}/v1/attendance/day/e?type=21`)
	                      .then(res => {
	                        if(res.data.code == '0'){
	                            this.emploee_list = res.data.result;
	                        }
	                      })
	                      .catch(err => {
	                          console.log(err);
	                      })
	
	         }
	
	          //根据typeID来并发请求
	          this.getAll(this.typeID);
	
	
	          if(this.typeID === 2){
	             this.getDepRank(this.typeID);
	          }
	
	       },
	       //员工考勤排名列表    11 日员工排名    21月员工排名
	       getEmploeeList(typeID){
	         this.$axios.get(`${this.URL_PREFIX}/v1/attendance/day/e?type=11`)
	                    .then(res => {
	                      if(res.data.code == '0'){
	                          this.lateList = res.data.result.slice(0,15);
	                      }
	                    })
	                    .catch(err => {
	                        console.log(err);
	                    })
	       },
	     getDepRank(typeID){
	         this.$axios.get(`${this.URL_PREFIX}/v1/attendance/day/e?type=${typeID}`)
	                    .then(res => {
	                      if(res.data.code == '0'){
	                          this.depList = res.data.result;
	                      }
	                    })
	                    .catch(err => {
	                        console.log(err);
	                    })
	       },
	       //点击跳转员工考勤详情展示页面
	       goDetail(event,item){
	         this.$router.push({
	           path:'/attendance/analys_emploee?id='+item.id,
	
	         })
	       }
	     }
	}
	</script>
	
	<style lang="scss" scoped>
	     .main{
	         width: 100%;
	         height: 100%;
	         background-color: #F0F2F5;
	         .dashboard{
