	                :loading="export_status"
	                >导出</Button
	              >
	            </Col>
	          </Row>
	        </Form>
	      </Card>
	      <Card>
	        <Row :style="{ marginBottom: '30px' }">
	          <h5 class="title-head">员工考勤</h5>
	          <Table
	            stripe
	            :columns="columns1"
	            :data="data1"
	            :loading="tableLoading"
	          ></Table>
	        </Row>
	        <Row :style="{ marginTop: '35px' }">
	          <Page
	            :style="{ float: 'right' }"
	            :total="this.totalCount"
	            :current="this.currentPage"
	            :page-size="this.pageSize"
	            size="small"
	            show-total
	            show-elevator
	            @on-change="changePage"
	          ></Page>
	        </Row>
	      </Card>
	    </div>
	  </div>
	</template>
	<script>
	import { mapState, mapActions } from "vuex";
	export default {
	  data() {
	    return {
	      tableLoading: false,
	      times: "",
	      keyword: "",
	      group: "",
	      data1: [],
	      searchLoading: false,
	      export_status: false,
	      currentPage: 1,
	      pageSize: 8,
	      totalCount: 3,
	      columns1: [
	        {
	          type: "selection",
	          width: 60,
	          align: "center"
	        },
	        {
	          title: "姓名",
	          key: "name",
	          align: "center"
	        },
	        {
	          title: "部门",
	          key: "dep",
	          align: "center"
	        },
	        {
	          title: "出勤天数",
	          key: "attend",
	          align: "center"
	        },
	        {
	          title: "休息天数",
	          key: "rest",
	          align: "center"
	        },
	        {
	          title: "迟到",
	          key: "late",
	          align: "center"
	        },
	        {
	          title: "旷工",
	          key: "absence",
	          align: "center"
	        },
	        {
	          title: "早退",
	          key: "quit",
	          align: "center"
	        },
	        {
	          title: "操作",
	          align: "center",
	          render: (h, params) => {
	            return h("div", [
	              h(
	                "p",
	                {
	                  style: {
	                    color: "#258ee9",
	                    cursor: "pointer"
	                  },
	                  on: {
	                    click: () => {
	                      this.$router.push({
	                        path: `/attendance/profile/${params.row.id}`,
	                        query: {
	                          times: this.times
	                        }
	                      });
	                    }
	                  }
	                },
	                "查看详情"
	              )
	            ]);
	          }
	        }
	      ],
	      data1: [
	        {
	          name: "李文",
	          dep: "商务部",
	          attend: 20,
	          rest: 2,
	          late: 3,
	          absence: 2,
	          quit: 1
	        },
	        {
	          name: "赵华",
	          dep: "财务部",
	          attend: 20,
	          rest: 2,
	          late: 3,
	          absence: 2,
	          quit: 1
	        },
	        {
	          name: "王伟",
	          dep: "研发部",
	          attend: 20,
	          rest: 2,
	          late: 3,
	          absence: 2,
	          quit: 1
	        }
	      ],
	      classData: []
	    };
	  },
	  mounted() {
	    //this.getClassList(1);
	  },
	  computed: {
	    ...mapState({
	      groupList: state => state.app.groupList
	    })
	  },
	  methods: {
	    //...mapActions(["getGroupList"]),
	
	    //用后端数据生成表格头结构
	    generateColumns(initColumns, renderColumns) {
	      renderColumns.forEach((thead, index) => {
	        initColumns[index].key = thead.key;
	        initColumns[index].title = thead.title;
	      });
	    },
	
	    //获取班级列表  表格头 表格数据
	    getClassList() {
	      this.tableLoading = true;
	      this.$axios
	        .get(
	          `${this.URL_PREFIX}/attendance/group/list?page=${
	            this.currentPage
	          }&size=${this.pageSize}&keyword=${this.keyword}&times=${
	            this.times
	          }&group=${this.group || ""}`
	        )
	        .then(res => {
	          if (res.data.code === "0") {
	            this.tableLoading = false;
	            this.data1 = res.data.result;
	            this.totalCount = res.data.total;
	            this.class_table_name = res.data.table_name;
	            this.generateColumns(this.columns1, res.data.table_header);
	
	            this.show = true;
	            if (this.data1.length < 1) {
	              this.tableLoading = false;
	              this.$Message.error("暂无搜索结果");
	            }
	          }
	        })
	        .catch(err => {
	          this.tableLoading = false;
	          this.$Message.error("服务器出错!");
	          console.log(err);
	        });
	    },
	
	    //获取班级详情
	    getClassDetail(id) {
	      this.$axios
	        .get(
	          `${this.URL_PREFIX}/attendance/group/info?id=${id}&page=${this.currentPage1}&size=${this.pageSize1}`
	        )
	        .then(res => {
	          if (res.data.code === "0") {
	            this.showClassInfoModal = true;
	            const { left, right, table_header, info } = res.data.result[0];
	
	            this.classTitle = res.data.table_name;
	            this.generateColumns(this.classColumns, table_header);
	            this.classData = info;
	            this.groupInfo = left;
	            this.groupInfos = right;
	            this.renderClassCloumns = true;
	          }
	        })
	        .catch(err => {
	          this.$Message.error("服务器出错!");
	          console.log(err);
	        });
	    },
	
	    //表格数据导出
	    Export() {
	      this.export_status = true;
	      this.$axios
	        .get(
	          `${this.URL_PREFIX}/attendance/group/export?keyword=${this.keyword}&group=${this.group}&times=${this.times}`
	        )
	        .then(res => {
	          if (res.data.code === "0") {
	            this.export_status = false;
	            this.$Message.success("导出成功");
	            setTimeout(() => {
	              window.location.href = `${res.data.url}`;
	            }, 500);
	          } else {
	            this.export_status = false;
	            this.$Message.error("导出失败");
	          }
	        })
	        .catch(err => {
	          this.$Message.error("服务器出错!");
	          this.export_status = false;
	          console.log(err);
	        });
	    },
	
	    //分页处理
	    changePage(index) {
	      this.currentPage = index;
	      this.getClassList();
	    },
	
	    //handle pagination
	    changePage1(index) {
	      this.currentPage1 = index;
	      this.getClassList();
	    },
	
	    //获取日期选择器的起始时间
	    handleTimeChange(datetime) {
	      this.times = datetime;
	    }
	  }
	};
	</script>
	
	<style scoped lang="scss">
	.list {
	  margin: 10px 20px;
	  height: 100%;
	  .title {
	    padding: 14px;
	    border-bottom: 1px solid #d9d9d9;
	  }
	}
	
	.statistic {
	  display: flex;
	  justify-content: space-between;
	  align-items: center;
	  margin-top: -3px;
	  margin-bottom: 15px;
	  > p {
	    span {
	      margin: 0 5px;
	    }
	  }
	  .ring {
	    width: 50px;
	    height: 50px;
	    display: inline-block;
	    border-radius: 50%;
