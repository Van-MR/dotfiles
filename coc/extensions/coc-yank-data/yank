	    //表格数据导出
	    Export() {
	      this.export_status = true;
	      this.$axios
	        .get(
	          `${this.URL_PREFIX}/visitor/list/export?keyword=${this.query.keyword}&times=${this.query.times}&type=${this.query.type}&sex=${this.query.sex}`
	        )
	        .then(res => {
	          if (res.data.code === "0") {
	            this.export_status = false;
	            this.$Message.success("导出成功");
	            setTimeout(() => {
	              window.location.href = `${res.data.url}`;
	            }, 500);
	          } else {
	            this.export_status = false;
	            this.$Message.error("导出失败");
	          }
	        })
	        .catch(err => {
	          this.$Message.error("服务器出错!");
	          this.export_status = false;
	          console.log(err);
	        });
	    },
