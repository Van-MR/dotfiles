322321188b8b9ce5a4c800b5de07ed93|file:///Users/aA/Desktop/webGl/samples/vtkModel/model.html|41|1|V|html
	  <script>
	    if (!Detector.webgl) Detector.addGetWebGLMessage()
	
	    var renderer, camera, scene, geometry, material, light
	
	    function initRenderer() {
	      renderer = new THREE.WebGLRenderer({ antialias: false })
	      renderer.setClearColorHex(0x000000, 1)
	      renderer.setSize(window.innerWidth, window.innerHeight)
	      var container = document.createElement('div')
	      container.appendChild(renderer.domElement)
	      document.body.appendChild(container)
	
	      stats = new Stats()
	      stats.domElement.style.position = 'absolute'
	      stats.domElement.style.top = '0px'
	      container.appendChild(stats.domElement)
	    }
	
	    function initCamera() {
	      camera = new THREE.PerspectiveCamera(
	        60,
	        window.innerHeight / window.innerHeight,
	        0.01,
	        1e10
	      )
	
	      camera.position.z = 0.2
	
	      controls = new THREE.TrackballControls(camera)
	
	      controls.rotateSpeed = 5.0
	      controls.zoomSpeed = 5
	      controls.panSpeed = 2
	
	      controls.noZoom = false
	      controls.noPan = false
	
	      controls.staticMoving = false
	      controls.dynamicDampingFactor = 0.3
	    }
	
	    function initScene() {
	      scen = new THREE.Scene()
	    }
	
	    function initLight() {
	      light = new THREE.DirectionalLight(0xffffff)
	      light.position.set(200, 200, 1000).normalize()
	
	      camera.add(light)
	      camera.add(light.target)
	    }
	
	    function initObject() {
	      material = new THREE.MeshLambertMaterial({
	        color: 0xffffff,
	        side: THREE.DoubleSide
	      })
	      var loader = new THREE.VTKLoader()
	      loader.addEventListener('load', function(event) {
	        geometry = event.content
	
	        mesh = new THREE.Mesh(geometry, material)
	        mesh.position.setY(-0.09)
	        scene.add(mesh)
	      })
	      loader.load('models/vtk/bunny.vtk')
	    }
	
	    function animate() {
	      requestAnimationFrame(animate)
	      controls.update()
	      renderer.render(scene, camera)
	      status.update()
	    }
	
	    function init() {
	      initRenderer()
	      initCamera()
	      initScene()
	      initLight()
	      initObject()
	      animate()
	    }
	
	    init()
