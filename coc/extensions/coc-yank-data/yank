	const getDeps = () => {
	    fetch(`${baseURL}/common/dict/group?type=1`)
	        .then(res => res.json())
	        .then(data => {
	            console.log(data)
	            initState.deps = data.result
	        })
	        .catch(err => {
	            console.log(err)
	        })
	}
	
	const getPurposes = () => {
	    fetch(`${baseURL}/common/dict/purpose`)
	        .then(res => res.json())
	        .then(data => {
	            initState.purposes = data.result
	            console.log(initState)
