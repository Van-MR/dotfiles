9f70a9363fe0145105bf03578ee1f00f|file:///Users/aA/Desktop/chibi/cb/src/views/logs/components/operateLog.vue|1|1|V|vue
	<template>
	  <div class="sysrole" v-track:18015.show>
	    <Card>
	      <Row :style="{ marginBottom: '30px' }">
	        <h5 class="title-head">操作列表</h5>
	        <Table
	          stripe
	          :columns="columns1"
	          :data="operateData"
	          :loading="tableLoading"
	        ></Table>
	      </Row>
	      <Row :style="{ marginTop: '35px' }">
	        <Page
	          :style="{ float: 'right' }"
	          :total="this.totalCount"
	          :current="this.currentPage"
	          :page-size="this.pageSize"
	          size="small"
	          show-total
	          show-elevator
	          @on-change="changePage"
	        ></Page>
	      </Row>
	    </Card>
	  </div>
	</template>
	<script>
	export default {
	  props: {
	    type: {
	      type: String,
	      required: true
	    },
	    times: {
	      type: String,
	      default: ""
	    },
	    keyword: {
	      type: String,
	      default: ""
	    }
	  },
	  data() {
	    return {
	      tableLoading: true,
	      currentPage: 1,
	      pageSize: 8,
	      totalCount: 0,
	      operateData: [],
	      columns1: [
	        {
	          key: "id",
	          title: "序号",
	          width: 80,
	          align: "center"
	        },
	        {
	          align: "center",
	          title: "操作用户",
	          width: 100,
	          key: "username"
	        },
	        {
	          align: "center",
	          title: "操作类型",
	          width: 100,
	          key: "type"
	        },
	        {
	          align: "center",
	          title: "操作模块",
	          key: "model"
	        },
	        {
	          align: "center",
	          title: "操作内容",
	          key: "content"
	        },
	        {
	          align: "center",
	          title: "操作结果",
	          key: "result"
	        },
	        {
	          align: "center",
	          title: "操作时间",
	          key: "times"
	        },
	        {
	          align: "center",
	          title: "地点",
	          width: 80,
	          key: "location"
	        },
	        {
	          align: "center",
	          title: "IP",
	          key: "ip"
	        }
	      ],
	      data1: []
	    };
	  },
	  mounted() {
	    this.getLogsList();
	  },
	  methods: {
	    getLogsList() {
	      this.tableLoading = true;
	      this.$axios
	        .get(
	          `${this.URL_PREFIX}/system/log?page=${this.currentPage}&size=${this.pageSize}&keyword=${this.keyword}&type=${this.type}&times=${this.times}`
	        )
	        .then(res => {
	          if (res.data.code === "0") {
	            this.tableLoading = false;
	            this.operateData = res.data.result;
	            this.totalCount = res.data.total;
	          } else {
	            this.$Message.error("服务器出错!");
	          }
	        })
	        .catch(err => {
	          this.tableLoading = false;
	          this.$Message.error("服务器出错!");
	          console.log(err);
	        });
	    },
	    //分页
	    changePage(index) {
	      this.currentPage = index;
	      this.getLogsList();
	    }
	  }
	};
	</script>
	
	<style scoped lang="scss">
	.sysrole {
	  padding: 10px 20px;
	  height: calc(100% - 40px);
	  overflow: scroll;
