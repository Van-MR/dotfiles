	     * @method { DELETE }
	     * @description 黑白名单移除
	     * @param { id } string  人员id
	     */
	    removeFromList(id) {
	      this.$axios
	        .delete(`${this.URL_PREFIX}/visitor/list`, {
	          data: {
	          id
	          }
	        })
	        .then(res => {
	          if (res.data.code === "0") {
	            this.$Message.success("移除成功");
	            this.getWhiteList();
	          } else {
	            this.$Message.errir("移除失败");
	          }
	        })
	        .catch(err => {
	          console.log(err);
	          this.$Message.errir("服务器出错");
	        });
	    },
