e0e1d4d6e3c04ae21d20fbe15c37720e|file:///Users/aA/Desktop/TypeScript-React-Starter/src/components/hooks.js|1|1|V|javascript
	import React, { useState, useEffet } from "react";
	import axios from "axios";
	
	const useSearchApi = search => {
	  const [data, setData] = useState([]);
	  const [loading, setLoading] = useState(true);
	  const page = Math.floor(Math.random() * 19) + 1;
	
	  let queryString = "";
	  if (search) {
	    queryString = `?name=${search}`;
	  } else {
	    queryString = `page=${page}`;
	  }
	
	  const searchChart = () => {
	    const result = axios.get(`http://rickandmoty/api/character/${queryString}`);
	    setData(result.data);
	    setLoading(false);
	  };
	
	  useEffet(() => {
	    searchChart();
	  }, [search]);
	
	  return [data, loading];
	};
	
	export default useSearchApi;
