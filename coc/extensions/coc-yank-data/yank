	[
	        {
	          title: "序号",
	          align: "center",
	          key: "id"
	        },
	        {
	          align: "center",
	          title: "设备名称",
	          key: "name"
	        },
	        {
	          align: "center",
	          title: "设备分组",
	          key: "group"
	        },
	        {
	          align: "center",
	          title: "设备编码",
	          key: "code"
	        },
	        {
	          align: "center",
	          title: "设备位置",
	          key: "position"
	        },
	        {
	          align: "center",
	          title: "状态",
	          key: "status",
	          render: (h, params) => {
	            let text = params.row.status;
	            let style = {};
	            text === "在线"
	              ? (style.color = "#60ca64")
	              : text === "离线"
	              ? (style.color = "#e84c3d")
	              : (style.color = "#c2c2c2");
	            return h("div", [
	              h(
	                "p",
	                {
	                  style,
	                  props: {
	                    type: "text"
	                  }
	                },
	                text
	              )
	            ]);
	          }
	        },
	        {
	          align: "center",
	          width: 200,
	          title: "添加时间",
	          key: "posttime"
	        },
	        {
	          align: "center",
	          title: "操作",
	          width: 200,
	          key: "action",
	          render: (h, params) => {
	            return h("div", [
	              h(
	                "P",
	                {
	                  props: {
	                    type: "success"
	                  },
	                  style: {
	                    display: "inline-block",
	                    color: "#268fe9",
	                    marginRight: "8px",
	                    cursor: "pointer"
	                  },
	                  on: {
	                    click: () => {
	                      this.getDeviceInfo(params.row.id);
	                    }
	                  }
	                },
	                "编辑"
	              ),
	              h(
	                "P",
	                {
	                  props: {
	                    type: "info"
	                  },
	                  style: {
	                    display: "inline-block",
	                    marginRight: "8px",
	                    color: "#e84c3d",
	                    cursor: "pointer"
	                  },
	                  on: {
	                    click: () => {
	                      this.$Modal.confirm({
	                        title: "提示",
	                        content: "<h5>确定要删除该数据吗?</h5>",
	                        onOk: () => {
	                          this.deleteDevice(params.row.id);
	                        }
	                      });
	                    }
	                  }
	                },
	                "删除"
	              )
	            ]);
	          }
	        }
