503a9ad68254382f1dd62f86e144354c|file:///Users/aA/Desktop/chibi/cb/src/store/modules/app.js|4|1|V|javascript
	const app = {
	  state: {
	    title: "", //系统名称
	    logoURI: "", //系统LOGO
	    nodes: [], //节点信息
	    menuList: [], //前端菜单栏
	    backendMenuList: [], //后端返回的原始菜单数据
	    openedMenuNameList: [],
	    groupList: [], //分组数据
	    roleList: [], //角色列表
	    showIconMenu: 0
	  },
	  getters: {},
	  mutations: {
	    updateTitle: (state, title) => {
	      localStorage.setItem("title", title);
	      state.title = title;
	      window.title = state.title;
	    },
	    saveNodes: (state, data) => {
	      localStorage.setItem("Nodes", JSON.stringify(data));
	      state.nodes = JSON.parse(localStorage.getItem("Nodes"));
	    },
	    updateMenulist: (state, data) => {
	      //序列化后储存
	      localStorage.setItem("menuList", JSON.stringify(data));
	      //解析成json对象读取
	      state.menuList = JSON.parse(localStorage.getItem("menuList"));
	    },
	    cleanMenu: state => {
	      localStorage.removeItem("menuList");
	      state.menuList = [];
	      localStorage.removeItem("backendMenuList");
	      state.backendMenuList = [];
	    },
	    saveBackendMenuList: (state, data) => {
	      localStorage.setItem("backendMenuList", JSON.stringify(data));
	      state.backendMenuList = JSON.parse(
	        localStorage.getItem("backendMenuList")
	      );
	    },
	    saveGroupList: (state, groupData) => {
	      localStorage.setItem("groupList", JSON.stringify(groupData));
	      state.groupList = JSON.parse(localStorage.getItem("groupList"));
	    },
	    saveRoleList: (state, roleData) => {
	      state.roleList = [];
	      localStorage.setItem("roleList", JSON.stringify(roleData));
	      state.roleList = JSON.parse(localStorage.getItem("roleList"));
	    },
	    //加载系统配置
	    loadSystemConfig(state, systemInfo) {
	      localStorage.setItem("systemInfo", JSON.stringify(systemInfo));
	      let config = JSON.parse(localStorage.getItem("systemInfo"));
	      state.title = config.name;
	      window.document.title = state.title;
	    },
	    updateLogoURI(state, url) {
	      localStorage.setItem("logo", JSON.stringify(url));
	      state.logoURI = JSON.parse(localStorage.getItem("logo"));
	    },
	    ToggleIconMenu(state, isShow) {
	      localStorage.setItem("showIconMenu", isShow);
	      state.showIconMenu = localStorage.getItem("showIconMenu");
	    },
	    setOpenedMenuList(state, openedMenuNameList) {
	      state.openedMenuNameList - openedMenuNameList;
	    }
	  },
	  actions: {
	    //openedmenulist
	    setCurrentPath({ dispatch, commit, state }, currentPageName) {
	      //let currentPath = [];
	      let openedMenuList = state.menuList.filter(menu => {
	        if (!menu.children) {
	          return menu.name === currentPageName;
	        } else {
	          return menu.children.some(child => {
	            return child.name === currentPageName;
	          });
	        }
	      });
