	    transformRecognizeType(type) {
	      switch (type) {
	        case "人脸检测":
	          return "1";
	        case "口罩检测":
	          return "2";
	        case "人脸+口罩":
	          return "5";
	        case "安全帽识别":
	          return "3";
	        case "抽烟检测":
	          return "4";
	      }
	    },
	
