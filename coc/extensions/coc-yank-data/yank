	      this.tableLoading = true;
	      this.$axios
	        .get(
	          `${this.URL_PREFIX}/current/access?page=${this.query.currentPage}&size=${this.query.pageSize}&keyword=${this.query.keyword}&status=${this.query.statu}&times=${this.query.times}`
	        )
	        .then(res => {
	          if (res.data.code == "0") {
	            this.tableLoading = false;
	            this.data1 = res.data.result;
	            this.totalCount = res.data.total;
	          } else {
	            this.tableLoading = false;
	            this.data1 = [];
	            this.$Message.warning("暂无搜索结果,请重新搜索");
	            return false;
	          }
	        })
	        .catch(err => {
	          this.tableLoading = false;
	          this.$Message.error("服务器出错...");
	          console.log(err);
	        });
