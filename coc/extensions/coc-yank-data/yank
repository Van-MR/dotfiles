b3e820ed69268edc63d536a07f60de18|file:///Users/aA/Desktop/chibi/cb/src/views/matrix/components/screens.vue|322|1|V|vue
	      this.paneData = this.paneData.map(item => {
	        return {
	          ...item,
	          selected: false
	        };
	      });
	    },
	    //构建窗格布局
	    generatePane() {
	      var target = document.getElementById("screens");
	      var W = target.offsetWidth;
	      var H = target.offsetHeight;
	      let { CanvasWidth, CanvasLong, Window } = this.paneData[0];
	      this.paneData = [];
	      let scaleWidth = W / CanvasLong;
	      let scaleHeight = H / CanvasWidth;
	      for (var i = 0; i < Window.length; i++) {
	        var pane_width =
	          (Number(Window[i].WindowX2) - Number(Window[i].WindowX1)) *
	          scaleWidth;
	        var pane_height =
	          (Number(Window[i].WindowY2) - Number(Window[i].WindowY1)) *
	          scaleHeight;
	        this.paneData = [
	          ...this.paneData,
	          {
	            id: Window[i].WindowId,
	            camera: Window[i].WindowCamera,
	            power: Window[i].WindowResolvingPower,
	            width: pane_width + "px",
	            height: pane_height + "px",
	            left: Window[i].WindowX1 * scaleWidth + "px",
	            top: Window[i].WindowY1 * scaleHeight + "px",
	            selected: false
	          }
	        ];
	      }
	      console.log(this.paneData);
	    },
	    searchOrigin() {
	      this.showContext = false;
	      this.selectPaneId = this.selectPaneId + "";
	      this.getStreams();
	      this.getWindowDict();
	      this.currentChoose = -1;
	      this.showOriginModal = true;
	    },
	    getStreams() {
	      this.$axios
	        .get(`${this.URL_Matrix}/matrix/stream?keyword=${this.keyword + ""}`)
	        .then(res => {
	          if (res.data.code === "0") {
	            this.data1 = res.data.result;
	          }
	        })
	        .catch(err => {
	          console.log(err);
	        });
	    },
	    getWindowDict() {
	      this.$axios
	        .get(`${this.URL_Matrix}/matrix/dict/window`)
	        .then(res => {
	          if (res.data.code === "0") {
	            this.windows = res.data.result;
	          }
	        })
	        .catch(err => {
	          console.log(err);
	        });
	    },
	    config() {
	      if (!this.selectPaneId) {
	        this.$Message.warning("请选择窗口");
	        return false;
	      }
	      this.$axios
	        .post(`${this.URL_Matrix}/matrix/stream`, {
	          windowId: this.selectPaneId + "",
	          stream_id: this.selectStreamId
	        })
	        .then(res => {
	          if (res.data.code === "0") {
	            this.showOriginModal = false;
	            this.getMatrixWindows();
	          } else {
	            this.showOriginModal = false;
	            this.selectData = [];
	            this.$Message.error(res.data.msg);
	          }
	        })
	        .catch(err => {
	          this.showOriginModal = false;
	          this.selectData = [];
	          this.$Message.error("服务器出错");
	          console.log(err);
	        });
	    },
	    //手动取消  数据清空
	    handlerCancel() {
	      this.selectData = [];
	      this.selectPaneId = "";
	      this.selectStreamId = "";
	    }
	  }
	};
	</script>
