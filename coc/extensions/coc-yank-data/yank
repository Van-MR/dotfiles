	    compileSetting(id, settingConfig) {
	      this.$axios
	        .put(`${this.URL_PREFIX}/statistic/attend`, { id, ...settingConfig })
	        .then(res => {
	          if (res.data.code == "0") {
	            this.$Message.success("编辑成功");
	            this.showAddComponent = false;
	            this.settingMode = "add";
	            this.getSettingList();
	          } else {
	            this.$Message.error(res.data.msg);
	          }
	        })
	        .catch(err => {
	          this.$Message.error("服务器出错!");
	          console.log(err);
	        });
	    },
