cf815174e4f8317f25ca5bed61f6f8c4|file:///Users/aA/Desktop/chibi/cb/src/views/statistics/components/attendanceSetting.vue|1|1|V|vue
	<template>
	  <div>
	    <div class="bread">
	      <p>
	        <Icon type="stats-bars" size="18" class="icon-bread"></Icon> 数据统计/
	        <Icon type="navicon" size="18" class="icon-bread"></Icon>
	        {{ $route.meta.title }}
	      </p>
	    </div>
	    <div class="list">
	      <Card
	        :style="{ marginTop: '15px', marginBottom: '20px', height: '80px' }"
	      >
	        <!-- 顶部搜索栏 -->
	        <Form
	          ref="searchroleform"
	          :style="{ marginTop: '5px', marginLeft: '-30px' }"
	          model="searchroleform"
	          :label-width="64"
	          inline
	        >
	          <Row>
	            <Col span="6" :style="{ paddingLeft: '18px' }">
	              <FormItem label="关键词">
	                <Input placeholder="请输入关键词搜索" v-model="keyword" />
	              </FormItem>
	            </Col>
	            <Col span="6">
	              <FormItem label="添加时间">
	                <DatePicker
	                  type="datetime"
	                  placeholder="请选择"
	                  :value="times"
	                  @on-change="handleTimeChange"
	                  style="width:160px"
	                ></DatePicker>
	              </FormItem>
	            </Col>
	            <Col span="5" :style="{ position: 'relative', left: '45px' }">
	              <Button
	                type="primary"
	                icon="ios-search"
	                v-track:18015.click="getClassList"
	                :loading="searchLoading"
	                >搜索</Button
	              >
	              <Button
	                type="primary"
	                icon="plus"
	                v-track:18015.click="Add"
	                >添加</Button
	              >
	            </Col>
	          </Row>
	        </Form>
	      </Card>
	      <Card>
	        <Row :style="{ marginBottom: '30px' }">
	          <h5 class="title-head">考勤组列表</h5>
	          <Table
	            stripe
	            :columns="columns1"
	            :data="data1"
	            :loading="tableLoading"
	          ></Table>
	        </Row>
	        <Row :style="{ marginTop: '35px' }">
	          <Page
	            :style="{ float: 'right' }"
	            :total="this.totalCount"
	            :current="this.currentPage"
	            :page-size="this.pageSize"
	            size="small"
	            show-total
	            show-elevator
	            @on-change="changePage"
	          ></Page>
	        </Row>
	      </Card>
	    </div>
	  </div>
	</template>
	<script>
	import { mapState, mapActions } from "vuex";
	export default {
	  data() {
	    return {
	      tableLoading: false,
	      times: "",
	      keyword: "",
	      group: "",
	      data1: [],
	      searchLoading: false,
	      export_status: false,
	      currentPage: 1,
	      pageSize: 8,
	      totalCount: 3,
	      columns1: [
	        {
	          title: "序号",
	          key: "id",
	          width: 60,
	          align: "center"
	        },
	        {
	          title: "名称",
	          key: "name",
	          align: "center"
	        },
	        {
	          title: "考勤时间",
	          key: "attendancetime",
	          align: "center"
	        },
	        {
	          title: "添加时间",
	          key: "addtime",
	          align: "center"
	        },
	        {
	          title: "操作",
	          align: "center",
	          render: (h, params) => {
	            return h("div", [
	              h(
	                "p",
	                {
	                  style: {
	                    display: "inline-block",
	                    marginRight: "15px",
	                    color: "#258ee9",
	                    cursor: "pointer"
	                  },
	                  on: {
	                    click: () => {
	                      //this.getClassDetail(params.row.id);
	                      this.groupName = params.row.group;
	                      this.getAttendenceDatail(this.groupName);
	                    }
	                  }
	                },
	                "编辑"
	              ),
	              h(
	                "p",
	                {
	                  style: {
	                    display: "inline-block",
	                    marginRight: "5px",
	                    color: "#ed462f",
	                    cursor: "pointer"
	                  },
	                  on: {
	                    click: () => {
	                      this.$Modal.confirm({
	                        title: "提示",
	                        content: "<h3>确定要删除吗?</h3>",
	                        onOk: () => {
	                          this.$axios
	                            .delete(`${this.URL_PREFIX}/user`, {
	                              data: {
	                                id: params.row.id + ""
	                              }
	                            })
	                            .then(res => {
	                              if (res.data.code == "0") {
	                                setTimeout(() => {
	                                  this.$Message.success("删除成功");
	                                }, 500);
	                              } else {
	                                setTimeout(() => {
	                                  this.$Message.error("删除失败");
	                                }, 500);
	                              }
	                            })
	                            .catch(error => {
	                              console.log(error);
	                            });
	                        }
	                      });
	                    }
	                  }
	                },
	                "删除"
	              )
	            ]);
	          }
	        }
	      ],
	      data1: [
	        {
	          id: 1,
	          name: "默认班次",
	          attendancetime: '2019-02-20 10:30:29',
	          addtime: '2019-02-20 10:30:29'
	        },
	        {
	          id: 2,
	          name: "确认班次",
	          attendancetime: '2019-02-20 10:30:29',
	          addtime: '2019-02-20 10:30:29'
	        }
	      ]
	    };
	  },
	  mounted() {
	    //this.getClassList(1);
	  },
	  computed: {
	    ...mapState({
	      groupList: state => state.app.groupList
	    })
	  },
	  methods: {
	    //...mapActions(["getGroupList"]),
	
	    //用后端数据生成表格头结构
	    generateColumns(initColumns, renderColumns) {
	      renderColumns.forEach((thead, index) => {
	        initColumns[index].key = thead.key;
	        initColumns[index].title = thead.title;
	      });
	    },
	
	    //获取班级列表  表格头 表格数据
	    getClassList() {
	      this.tableLoading = true;
	      this.$axios
	        .get(
	          `${this.URL_PREFIX}/attendance/group/list?page=${
	            this.currentPage
	          }&size=${this.pageSize}&keyword=${this.keyword}&times=${
	            this.times
	          }&group=${this.group || ""}`
	        )
	        .then(res => {
	          if (res.data.code === "0") {
	            this.tableLoading = false;
	            this.data1 = res.data.result;
	            this.totalCount = res.data.total;
	            this.class_table_name = res.data.table_name;
	            this.generateColumns(this.columns1, res.data.table_header);
	
	            this.show = true;
	            if (this.data1.length < 1) {
	              this.tableLoading = false;
	              this.$Message.error("暂无搜索结果");
	            }
	          }
	        })
	        .catch(err => {
	          this.tableLoading = false;
	          this.$Message.error("服务器出错!");
	          console.log(err);
	        });
	    },
	
	    //获取班级详情
	    getClassDetail(id) {
	      this.$axios
	        .get(
	          `${this.URL_PREFIX}/attendance/group/info?id=${id}&page=${this.currentPage1}&size=${this.pageSize1}`
	        )
	        .then(res => {
	          if (res.data.code === "0") {
	            this.showClassInfoModal = true;
	            const { left, right, table_header, info } = res.data.result[0];
	
	            this.classTitle = res.data.table_name;
	            this.generateColumns(this.classColumns, table_header);
	            this.classData = info;
	            this.groupInfo = left;
	            this.groupInfos = right;
	            this.renderClassCloumns = true;
	          }
	        })
	        .catch(err => {
	          this.$Message.error("服务器出错!");
	          console.log(err);
	        });
	    },
	
	    //添加
	    Add() {
	    },
	
	    //分页处理
	    changePage(index) {
	      this.currentPage = index;
	      this.getClassList();
	    },
	
	    //handle pagination
	    changePage1(index) {
	      this.currentPage1 = index;
	      this.getClassList();
	    },
	
	    //获取日期选择器的起始时间
	    handleTimeChange(datetime) {
	      this.times = datetime;
	    }
	  }
	};
	</script>
	
