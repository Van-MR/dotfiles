	    checkAllGroupChange_Add(selected) {
	      selected.child.map(c => (c.selected = "0"));
	
	      selected.selectedData.forEach(option => {
	        var a = selected.child.find(items => items.name == option);
	        a.selected = "1";
	      });
	
	      console.log(selected);
	      console.log(this.addNodeList);
	    },
