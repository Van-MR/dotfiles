	    fetchAttendanceList(id) {
	      this.loading = true;
	      this.$axios
	        .get(
	          `${this.URL_PREFIX}/statistic/person/table?id=${id}&page=${this.currentPage}&size=${this.pageSize}`
	        )
	        .then(res => {
	          if (res.data.code === "0") {
	            this.loading = false;
	            this.totalCount = res.data.total;
	            const { table_name, table_header, info } = res.data.result[0];
	            this.data1 = info;
	            this.table_name = table_name;
	            this.generateColumns(this.columns, table_header);
	            this.istableRender = true;
	          }
	        })
	        .catch(error => {
	          this.loading = false;
	          console.log(error);
	        });
	    },
	
