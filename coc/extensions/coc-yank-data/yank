feb825d2bff00bab2af5264b3d579ed9|file:///Users/aA/Desktop/epidemic-prevention/src/pages/Entry.js|152|1|V|javascript
	  //从支付宝端扫码 获取auth_code
	  const getCodeFromAliPay = () => {
	    var code = getParamsFromAliPay();
	
	    if (id && code) {
	      id = id.split("&")[0];
	      getQrCodeInfo(id);
	    }
	
	    //如果是支付宝进入，根据auth_code获取openId , 否则重定向到授权页面
	    fetch(`${BASE_URL}/phone/alipay/access?code=${code}`)
	      .then((res) => res.json())
	      .then((data) => {
	        if (data.code === "0") {
	          setToken(data.token);
	          localStorage.setItem("token", data.token);
	          setOnlyID(data.only_id);
	          fetchUserInfo();
	        } else if (data.code === "-10011") {
	          window.location.replace(
	            "https://openauth.alipay.com/oauth2/publicAppAuthorize.htm?app_id=" +
	              data.only_id +
	              "&scope=auth_user" +
	              "&redirect_uri=" +
	              window.location.href
	          );
	        }
	      })
	      .catch((e) => {
	        console.log(e);
	      });
	  };
bd7f9be155ba8a07c70ae205afe46e59|file:///Users/aA/Desktop/epidemic-prevention/src/pages/Entry.js|139|1|V|javascript
	  //获取uri重定向地址中的code
	  const getParamsFromAliPay = () => {
	    let code;
	    let location = window.location.href;
	    if (location.includes("auth_code=")) {
	      code = location.split("auth_code=")[1];
	      return code;
	    }
	
	    code = "";
	    return code;
	  };
a1df67e31ab7fdea1b5f8e655e51b035|file:///Users/aA/Desktop/epidemic-prevention/src/pages/Entry.js|84|1|V|javascript
	  //获取用户信息
	  const fetchUserInfo = useCallback(async () => {
	    try {
	      const res = await fetch(`${BASE_URL}/person/info`, {
	        method: "GET",
	        headers: {
	          "Content-Type": "application/json",
	          Authorization: "JWT " + localStorage.getItem("token"),
	        },
	      });
	      const jsonData = await res.json();
	      if (jsonData.code === "0") {
	        setInfo(jsonData.result[0]);
	        props.history.push({
	          pathname: "/access",
	          state: { data: jsonData.result[0] },
	        });
	      }
	    } catch (e) {
	      console.log(e);
	    }
	  });
6b1b9cfb830e0e457210cb34d570a7e4|file:///Users/aA/Desktop/epidemic-prevention/src/pages/Entry.js|121|1|V|javascript
	  //获取uri重定向地址中的code
	  const getParams = () => {
	    let code;
	    let location = window.location.href;
	    let reg = /\&code=/;
	    let result = reg.test(location);
	    if (result) {
	      let k = location.split("?")[1];
	      let d = k.split("&")[1];
	      let f = d.substr(0, 5);
	      code = d.replace(f, "");
	      return code;
	    }
	
	    code = "";
	    return code;
	  };
