	<template>
	  <div>
	    <div class="bread">
	      <p>
	        <Icon type="android-walk" size="18" class="icon-bread"></Icon>通行管理/
	        <Icon type="navicon" size="18" class="icon-bread"></Icon>
	        {{ $route.meta.title }}
	      </p>
	    </div>
	    <div class="list">
	      <Card :style="{ marginTop: '15px', marginBottom: '20px', height: '80px' }">
	        <!-- 顶部搜索栏 -->
	        <Form :style="{ marginTop: '5px', marginLeft: '-30px' }" :label-width="64" inline>
	          <Row>
	            <Col span="6" :style="{ paddingLeft: '18px' }">
	              <FormItem label="关键词">
	                <Input placeholder="事件名称、内容" v-model="query.keyword" clearable />
	              </FormItem>
	            </Col>
	            <Col span="6">
	              <FormItem label="分析状态:" :style="{ width: '250px' }">
	                <Select v-model="query.type" style="width:150px" clearable>
	                  <Option v-for="item in types" :value="item.id" :key="item.id">{{ item.name }}</Option>
	                </Select>
	              </FormItem>
	            </Col>
	            <Col span="6">
	              <FormItem label="分析时间">
	                <DatePicker
	                  type="datetime"
	                  placeholder="请选择"
	                  :value="query.times"
	                  @on-change="handleTimeChange"
	                  style="width:160px"
	                ></DatePicker>
	              </FormItem>
	            </Col>
	            <Col span="6" :style="{ position: 'relative', left: '45px' }">
	              <Button
	                type="primary"
	                icon="ios-search"
	                v-track:18015.click="getEventList"
	                :loading="searchLoading"
	              >搜索</Button>
	              <Button
	                type="primary"
	                icon="ios-cloud-upload-outline"
	                v-track:18015.click="Export"
	                :loading="export_status"
	              >导出</Button>
	            </Col>
	          </Row>
	        </Form>
	      </Card>
	      <Card>
	        <Row :style="{ marginBottom: '30px' }">
	          <h5 class="title-head">事件列表</h5>
	          <Table stripe :columns="columns1" :data="data1" :loading="tableLoading"></Table>
	        </Row>
	        <Row :style="{ marginTop: '35px' }">
	          <Page
	            :style="{ float: 'right' }"
	            :total="this.totalCount"
	            :current="this.query.currentPage"
	            :page-size="this.query.pageSize"
	            size="small"
	            show-total
	            show-elevator
	            @on-change="changePage"
	          ></Page>
	        </Row>
	      </Card>
	    </div>
	    <Modal v-model="showTrafficInfo" :mask-closable="false" title="通行详情" width="450px">
	      <div class="modal_content">
	        <div
	          :style="{
	            display: 'flex',
	            justifyContent: 'space-between',
	            alignItems: 'center'
	          }"
	        >
	          <img
	            v-for="(pic, index) in trafficInfo.pic"
	            :src="pic"
	            :key="index"
	            :style="{ width: '30%' }"
	            alt="record_pic"
	          />
	        </div>
	        <div class="modal_medium">
	          <Form>
	            <Row>
	              <Col span="12">
	                <div class="formItem">
	                  <p class="label">姓名</p>
	                  <p>{{ trafficInfo.name }}</p>
	                </div>
	                <div class="formItem">
	                  <p class="label">通行设备</p>
	                  <p>{{ trafficInfo.device }}</p>
	                </div>
	                <div class="formItem">
	                  <p class="label">所属部门</p>
	                  <p>{{ trafficInfo.group }}</p>
	                </div>
	                <div class="formItem">
	                  <p class="label">通行时间</p>
	                  <p>{{ trafficInfo.times }}</p>
	                </div>
	              </Col>
	              <Col span="12">
	                <div class="formItem">
	                  <p class="label">身份</p>
	                  <p>{{ trafficInfo.type }}</p>
	                </div>
	                <div class="formItem">
	                  <p class="label">进出方向</p>
	                  <p>{{ trafficInfo.direction }}</p>
	                </div>
	                <div class="formItem">
	                  <p class="label">通行状态</p>
	                  <p :class="{ active: trafficInfo.status === '在访' }">{{ trafficInfo.status }}</p>
	                </div>
	              </Col>
	            </Row>
	          </Form>
	        </div>
	      </div>
	      <div slot="footer"></div>
	    </Modal>
	  </div>
	</template>
	<script>
	import { mapState, mapActions } from "vuex";
	export default {
	  data() {
	    return {
	      tableLoading: false,
	      searchLoading: false,
	      export_status: false,
	      showTrafficInfo: false,
	      types: [
	        { id: 0, name: "进行中" },
	        { id: 1, name: "已完成" }
	      ],
	      trafficInfo: {},
	      query: {
	        keyword: "",
	        types: "",
	        times: "",
	        currentPage: 1,
	        pageSize: 8
	      },
	      totalCount: 0,
	      data1: [],
	      columns1: [
	        {
	          title: "序号",
	          key: "id",
	          width: 80,
	          align: "center"
	        },
	        {
	          title: "事件名称",
	          key: "name",
	          align: "center"
	        },
	        {
	          title: "算法类型",
	          key: "type",
	          align: "center"
	        },
	        {
	          title: "设备",
	          key: "device",
	          align: "center"
	        },
	        {
	          title: "分析状态",
	          key: "times",
	          align: "center"
	        },
	        {
	          title: "分析时间",
	          key: "times",
	          align: "center"
	        },
	        {
	          title: "操作",
	          align: "center",
	          render: (h, params) => {
	            return h("div", [
	              h(
	                "p",
	                {
	                  style: {
	                    color: "#258ee9",
	                    cursor: "pointer"
	                  },
	                  on: {
	                    click: () => {
	                      //this.getTrafficInfo(params.row.id);
	                    }
	                  }
	                },
	                "重新分析"
	              ),
	              h(
	                "p",
	                {
	                  style: {
	                    color: "#258ee9",
	                    cursor: "pointer"
	                  },
	                  on: {
	                    click: () => {
	                      //this.getTrafficInfo(params.row.id);
	                    }
	                  }
	                },
	                "查看详情"
	              )
	            ]);
	          }
	        }
	      ],
	      data1: []
	    };
	  },
	  mounted() {
	    this.getEventList();
	  },
	  methods: {
	    //获取事件列表
	    getEventList() {
	      this.tableLoading = true;
	      this.$axios
	        .get(
	          `${this.URL_PREFIX}/device/log?page=${this.query.currentPage}&size=${
	            this.query.pageSize
	          }&keyword=${this.query.keyword || ""}&type=${this.query.type ||
	            ""}&times=${this.query.times || ""}`
	        )
	        .then(res => {
	          if (res.data.code == "0") {
	            this.tableLoading = false;
	            this.data1 = res.data.result;
	            this.totalCount = parseInt(res.data.total);
	          } else {
	            this.tableLoading = false;
	            this.data1 = [];
	            this.$Message.warning("暂无搜索结果,请重新搜索");
	            return false;
	          }
	        })
	        .catch(err => {
	          this.tableLoading = false;
	          this.$Message.error("服务器出错...");
	          console.log(err);
	        });
	    },
	
	    //trafficInfo 详情
	    getTrafficInfo(id) {
	      this.$axios
	        .get(`${this.URL_PREFIX}/current/log/info?id=${id}`)
	        .then(res => {
	          if (res.data.code === "0") {
	            this.showTrafficInfo = true;
	            const {
	              name,
	              pic,
	              device,
	              group,
	              direction,
	              type,
	              times,
	              status
	            } = res.data.result[0];
	
	            this.trafficInfo.name = name;
	            this.trafficInfo.pic = JSON.parse(pic);
	            this.trafficInfo.device = device;
	            this.trafficInfo.direction = direction;
	            this.trafficInfo.group = group;
	            this.trafficInfo.type = type;
	            this.trafficInfo.times = times;
	            this.trafficInfo.status = status;
	          } else {
	            this.$Message.error(res.data.message);
	          }
	        })
	        .catch(err => {
	          this.$Message.error("服务器出错!");
	          console.log(err);
	        });
	    },
	    //表格数据导出
	    Export() {
	      this.export_status = true;
	      this.$axios
	        .get(
	          `${this.URL_PREFIX}/device/log/export?keyword=${
	            this.query.keyword
	          }&type=${this.query.type || ""}&times=${this.query.times}`
	        )
	        .then(res => {
	          if (res.data.code === "0") {
	            this.export_status = false;
	            this.$Message.success("导出成功");
	            setTimeout(() => {
	              window.location.href = `${res.data.url}`;
	            }, 500);
	          } else {
	            this.export_status = false;
	            this.$Message.error("导出失败");
	          }
	        })
	        .catch(err => {
	          this.$Message.error("服务器出错!");
	          this.export_status = false;
	          console.log(err);
	        });
	    },
	
	    //分页处理
	    changePage(index) {
	      this.query.currentPage = index;
	      this.getEventList();
	    },
	
	    //获取日期选择器的起始时间
	    handleTimeChange(datetime) {
	      this.query.times = datetime;
	    }
	  }
	};
	</script>
	
