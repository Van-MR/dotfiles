	export function 
32a0af985de87b882f1a547ab0fb305b|file:///Users/aA/Desktop/chibi/cb/src/views/cam/index.vue|44|1|V|vue
	deteleObject(obj) {
	    var uniques = [];
	    var stringify = {};
	    for (var i = 0; i < obj.length; i++) {
	        var keys = Object.keys(obj[i]);
	        keys.sort(function(a, b) {
	            return (Number(a) - Number(b));
	        });
	        var str = '';
	        for (var j = 0; j < keys.length; j++) {
	            str += JSON.stringify(keys[j]);
	            str += JSON.stringify(obj[i][keys[j]]);
	        }
	        if (!stringify.hasOwnProperty(str)) {
	            uniques.push(obj[i]);
	            stringify[str] = true;
	        }
	    }
	    uniques = uniques;
	    return uniques;
	}
