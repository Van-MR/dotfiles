	    //获取所有节点列表
	    getNodes() {
	      this.$axios
	        .get(`${this.URL_PREFIX}/user/role/node`)
	        .then(res => {
	          if (res.data.code == "0") {
	            this.$store.commit("saveNodes", res.data.result);
	          }
	        })
	        .catch(err => {
	          console.log(err);
	        });
	    },
