ff76ad5c90aab8ec9c2cf4fe75e3d760|file:///Users/aA/Desktop/chibi/cb/src/views/statistics/components/attendanceList.vue|200|1|V|vue
	    this.getAttendanceList();
	  },
	  methods: {
	    //获取考勤列表
	    getAttendanceList() {
	      this.tableLoading = true;
	      this.$axios
	        .get(
	          `${this.URL_PREFIX}/statistic/attend/list?page=${this.currentPage}&size=${this.pageSize}&keyword=${this.keyword}&times=${this.times}`
	        )
	        .then(res => {
	          if (res.data.code === "0") {
	            this.tableLoading = false;
	            this.data1 = res.data.result;
	            this.totalCount = res.data.total;
	            if (this.data1.length < 1) {
	              this.tableLoading = false;
	              this.$Message.error("暂无搜索结果");
	            }
	          }
	        })
	        .catch(err => {
	          this.tableLoading = false;
	          this.$Message.error("服务器出错!");
	          console.log(err);
	        });
	    },
	
	    //考勤列表表格数据导出
	    Export() {
	      this.export_status = true;
	      this.$axios
	        .get(
	          `${this.URL_PREFIX}/attendance/group/export?keyword=${this.keyword}&group=${this.group}&times=${this.times}`
	        )
	        .then(res => {
	          if (res.data.code === "0") {
	            this.export_status = false;
	            this.$Message.success("导出成功");
	            setTimeout(() => {
	              window.location.href = `${res.data.url}`;
	            }, 500);
	          } else {
	            this.export_status = false;
	            this.$Message.error("导出失败");
	          }
	        })
	        .catch(err => {
	          this.$Message.error("服务器出错!");
	          this.export_status = false;
	          console.log(err);
	        });
	    },
	
	    //分页处理
	    changePage(index) {
	      this.currentPage = index;
	      this.getAttenceList();
	    },
	
	    //获取日期选择器的起始时间
	    handleTimeChange(datetime) {
	      this.times = datetime;
	    }
	  }
	};
	</script>
	
