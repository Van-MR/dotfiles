	          })
	          .then(res => {
	            if (res.data.code == "0") {
	              this.getUserList();
	              this.$Message.success("新增用户成功");
	              this.resetAddUserForm();
	            } else if (res.data.code == "10001") {
	              setTimeout(() => {
	                this.$Notice.error({
	                  title: "消息提示",
	                  render: h => {
	                    return h("span", ["该用户 ", h("a", "已存在")]);
	                  }
	                });
	              }, 500);
	              this.resetAddUserForm();
	            } else {
	              setTimeout(() => {
	                this.$Message.error("新增用户失败");
	                this.resetAddUserForm();
	              }, 500);
	            }
	          })
	          .catch(error => {
	            console.log(error);
