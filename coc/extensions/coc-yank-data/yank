608a7ff2af231ac3c357836399908ca6|file:///Users/aA/Desktop/chibi/cb/src/views/statistics/charts/lines.vue|1|1|V|vue
	<template>
	  <div :class="className" :style="{height:height,width:width,marginTop:'-1px'}"></div>
	</template>
	
	<script>
	import echarts from 'echarts'
	require('echarts/theme/macarons') // echarts theme
	
	const animationDuration = 1500
	
	export default {
	  props: {
	    chartData: {
	      type: Object
	    },
	    className: {
	      type: String,
	      default: 'lines'
	    },
	    text:{
	      type: String,
	      default: 'chart'
	    },
	    width: {
	      type: String,
	      default: '100%'
	    },
	    height: {
	      type: String,
	      default: '270px'
	    }
	  },
	  data() {
	    return {
	      chart: null
	    }
	  },
	  watch: {
	    chartData: {
	      deep: true,
	      handler(val) {
	        this.setOptions(val)
	      }
	    }
	  },
	  beforeDestroy() {
	    if (!this.chart) {
	      return
	    }
	    this.chart.dispose()
	    this.chart = null
	  },
	  mounted() {
	    this.initChart()
	  },
	  methods: {
	      setOptions({ xData,expectedData } = {}) {
	        this.chart = echarts.init(this.$el, 'macarons')
	        this.chart.setOption({
	          color: ['#D53A35', '#E98F6F', '#6AB0B8', '#334B5C'],
	          tooltip: {
	              trigger: 'axis',
	              //formatter: "{b} <br> 合格率: {c}%"
	          },
	          legend: {
	              data: ['正常', '迟到', '早退', '脱岗', '缺勤']
	          },
	          grid: {
	              left: '3%',
	              right: '4%',
	              bottom: '3%',
	              containLabel: true
	          },
	          xAxis: {
	              type: 'category',
	              boundaryGap: false,
	              name: '时间',
	              nameTextStyle: {
	                color: '#d4ffff'
	              },
	              axisLine: {
	                lineStyle: {
	                    color: '#0B4CA9'
	                }
	              },
	              axisTick: {
	                show: false,
	              },
	              axisLabel: {
	                show: true,
	                textStyle: {
	                    color: "#FFF",
	                    fontSize: 12,
	                },
	                //interval:0,
	                //rotate:30
	              },
	              data: xData
	          },
	          yAxis: {
	              type: 'value',
	              name: '单位:(次)'
	          },
	          series:[
	            {
	                name: '正常',
	                type: 'line',
	                data: expectedData.map(item => item[0].normal)
	            },
	            {
	                name: '迟到',
	                type: 'line',
	                data: expectedData.map(item => item[0].late)
	            },
	            {
	                name: '缺勤',
	                type: 'line',
	                data: expectedData.map(item => item[0].absence)
	            },
	            {
	                name: '请假',
	                type: 'line',
	                data: expectedData.map(item => item[0].leave)
	            },
	            {
	                name: '早退',
	                type: 'line',
	                data: expectedData.map(item => item[0].quit)
	            }
	         ]
	      })
	
	      },
	      initChart() {
	      this.chart = echarts.init(this.$el, 'macarons')
	      this.setOptions(this.chartData);
	      var _this = this
	      window.onresize = function(){
	         _this.chart.resize()
	      }
	      window.onload = function (){
	        _this.chart.resize()
	      }
	     }
	  }
	}
	</script>
	<style scoped>
	.lines {
	    position: relative;
	    top: 20px;
	}
	</style>
