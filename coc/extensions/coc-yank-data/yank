	<script>
	import Info from './analysInfo.vue'
	import Pie from '../charts/pie.vue'
	import Pie1 from '../charts/pie1.vue'
	import Bar  from '../charts/bar.vue'
	import Bar1  from '../charts/bar1.vue'
	import Bar2  from '../charts/bar2.vue'
	import Lines from '../charts/lines.vue'
	
	export default {
	     data(){
	       return {
	          msg:'考勤分析',
	          chartData:{},
	          attendanceData:{},
	          currentIndex:0,
	          showInfo: false,
	          attendanceInfo:{
	            normal:0,
	            late:0,
	            quit:0,
	            absence:0,
	            leave:0
	          },
	          lineChartData: {
	           xData:[],
	           expectedData: []
	          },
	          lineChartData1: {
	           xData:[],
	           expectedData: []
	          },
	          lineChartData2: {
	           xData:[],
	           expectedData: []
	          },
	          tabList:['每天','每月'],
	          lateList:[],
	          depList:[],
	          emploee_list:[]
	       }
	     },
	     components:{
	        Pie,
	        Pie1,
	        Bar,
	        Bar1,
	        Bar2,
	        Lines,
	        Info
	     },
	     created(){
	     },
	     mounted(){
	           this.getDayStatisticData();
	     },
	     methods:{
	         // get all part data by day 
	         getDayStatisticData() {
	             this.$axios.get(`${this.URL_PREFIX}/v1/statistic/day`)
	                 .then(res => {
	                     if(res.data.code === '0'){
	                         // haader part statistic info
	                         const { normal, absence, late, quit, leave } = res.data.result[0].header[0];
	                         this.attendanceInfo.normal = normal;
	                         this.attendanceInfo.absence = absence;
	                         this.attendanceInfo.late = late;
	                         this.attendanceInfo.quit = quit;
	                         this.attendanceInfo.leave = leave;
	
	                         // day analys (pie)
	                         this.lineChartData.xData = res.data.result[0].day.map(item => item.name);
	                         this.lineChartData.expectedData = res.data.result[0].day.map(item => {
	                             return {
	                                 name:item.name,
	                                 value:item.total
	                             }
	                         });
	
	                         //department attendeance analys data  (bar) 
	                         this.lineChartData1.xData = res.data.result[0].group.map(item => item.name);
	                         this.lineChartData1.expectedData = res.data.result[0].group.map(item => item.info);
	                         
	                         //day attendeance  rate  (bar)
	                         this.lineChartData2.xData = res.data.result[0].rate.map(item => item.name);
	                         this.lineChartData2.expectedData = res.data.result[0].rate.map(item => {
	                             return {
	                                 name:item.name,
	                                 value:item.rate
	                             }
	                         });
	
	                         //emploee late list
	                         this.lateList = res.data.result[0].lateList;
	
	                     }
	                 }).catch(err => {
	                     console.log(err);
	                 })
	         },
	
	         //get all parts data by month
	         getMonthStatisticData(){
	             this.$axios.get(`${this.URL_PREFIX}/v1/statistic/month`)
	                 .then(res => {
	                     if(res.data.code === '0'){
	                         // haader part statistic info
