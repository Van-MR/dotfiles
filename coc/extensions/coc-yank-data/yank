7d859dae01ca5bdbbafeca43cd23c75f|file:///Users/aA/Desktop/chibi/cb/src/views/logs/components/expectedLog.vue|125|1|V|vue
	    //导出表格数据
	    Export() {
	      this.export_status = true;
	      this.$axios
	        .post(
	          `${this.URL_PREFIX}/system/log?keyword=${this.keyword}&type=${this.type}&times=${this.times}`
	        )
	        .then(res => {
	          if (res.data.code === "0") {
	            this.export_status = false;
	            this.$Message.success("导出成功");
	            setTimeout(() => {
	              window.location.href = `${res.data.url}`;
	            }, 500);
	          } else {
	            this.export_status = false;
	            this.$Message.error("导出失败");
	          }
	        })
	        .catch(err => {
	          this.$Message.error("服务器出错!");
	          this.export_status = false;
	          console.log(err);
	        });
	    },
