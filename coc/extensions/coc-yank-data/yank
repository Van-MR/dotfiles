	<script>
	export default {
	  name: "addAttendanceSetting",
	  props: {
	    settingMode: {
	      type: String,
	      required: true
	    },
	    settingConfigs: {
	      type: Object,
	      required: true
	    },
	    addSetting: {
	      type: Function
	    },
	    compileSetting: {
	      type: Function
	    }
	  },
	  data() {
	    return {
	      dicts: [],
	      totalCount: 0,
	      pageSize: 10,
	      currentPage: 1,
	      keyword: "",
	      currentChoose: "",
	      currentSelect: -1,
	      selectTimes: "",
	      selectID: -1,
	      selectData: [],
	      compileDiffData: [],
	      showArrangeModal: false,
	      configs: {
	        name: "",
	        shifts: "",
	        legal: false
	      },
	      columns: [
	        {
	          type: "selection",
	          width: 60
	        },
	        {
	          title: "工作日",
	          key: "workday",
	          align: "center"
	        },
	        {
	          title: "时间段",
	          key: "times",
	          align: "center"
	        },
	        {
	          title: "操作",
	          align: "center",
	          render: (h, params) => {
	            return h("div", [
	              h(
	                "p",
	                {
	                  style: {
	                    cursor: "pointer",
	                    color: "#258ee9"
	                  },
	                  on: {
	                    click: () => {
	                      this.currentSelect = params.row.index;
	                      this.showArrangeModal = true;
	                    }
	                  }
	                },
	                "更改班次"
	              )
	            ]);
	          }
	        }
	      ],
	      columns1: [
	        {
	          title: "选择",
	          key: "id",
	          width: 60,
	          align: "center",
	          render: (h, params) => {
	            let id = params.row.id;
	            let flag = false;
	            if (this.currentChoose === id) {
	              flag = true;
	            } else {
	              flag = false;
	            }
	            let self = this;
	            return h("div", [
	              h("Radio", {
	                props: {
	                  value: flag
	                },
	                on: {
	                  "on-change": () => {
	                    self.currentChoose = id;
	                    let item = this.dicts.find(item => item.id === id);
	                    if (item) {
	                      this.selectTimes = item.name;
	                      this.selectID = id;
	                    }
	                  }
	                }
	              })
	            ]);
	          }
	        },
	        {
	          title: "班次",
	          key: "name",
	          width: 120,
	          align: "center"
	        },
	        {
	          title: "时间段",
	          key: "times",
	          align: "center"
	        }
	      ],
	      data: [
	        {
	          workday: "周一",
	          times: ""
	        },
	        {
	          workday: "周二",
	          times: ""
	        },
	        {
	          workday: "周三",
	          times: ""
	        },
	        {
	          workday: "周四",
	          times: ""
	        },
	        {
	          workday: "周五",
	          times: ""
	        },
	        {
	          workday: "周六",
	          times: ""
	        },
	        {
	          workday: "周日",
	          times: ""
	        }
	      ],
	      data1: []
	    };
	  },
	  created() {
	    if (this.settingMode === "add") {
	      this.getDaySchemes();
	    } else {
	      this.data = [];
	      const { name, shifts, legal } = this.settingConfigs;
	      this.configs.name = name;
	      this.configs.legal = legal;
	
	      shifts.forEach((item, index) => {
	        const row = {
	          id: item.shift[0].id,
	          workday: item.name,
	          times: item.shift[0].value,
	          index: index
	        };
	        this.data.push(row);
	      });
	    }
	  },
	  mounted() {
	    this.getClassesList();
	    this.getDicts();
	    this.checkSelected();
	  },
	  methods: {
	    checkSelected() {
	      if (this.settingMode === "edit") {
	        this.data = this.data.map((item, index) => {
	          if (item.id >= 0) {
	            return { ...item, _checked: true };
	          } else {
	            return { ...item, _checked: false };
	          }
	        });
	        this.selectData = this.data.filter(item => item._checked == true);
	        for (var i = 0; i < this.data.length; i++) {
	          if (this.data[i]._checked == true) {
	            this.$refs.table.objData[i]._isChecked = true;
	          } else {
	            this.$refs.table.objData[i]._isChecked = false;
	          }
	        }
	      }
	    },
	    getArrDifference(arr1, arr2) {
	      return arr1.filter(item => {
	        return !arr2.map(item => item.workday).includes(item.workday);
	      });
	    },
	    transDiff(Arr) {
	      Arr.forEach(item => {
	        if (item.id != 0) {
	          item.id = -item.id + "";
	        } else {
	          item.id = 0;
	        }
	      });
	      return Arr;
	    },
	    //正转负 (之前选中的数据变成未选中状态)
	    transCheckedToUncheked(Arr) {
	      Arr.forEach(item => {
	        if (item.id > 0) {
	          item.id = -item.id;
	        }
	      });
	      return Arr;
	    },
	    //负转正 (之前未选中的数据变成选中状态)
	    retransCheckedToUncheked(Arr) {
	      Arr.forEach(item => {
	        if (item.id < 0) {
	          item.id = Math.abs(item.id);
	        }
	      });
	      return Arr;
	    },
	
	    retransDiff(Arr) {
	      Arr.forEach(item => {
	        if (item.id < 0) {
	          item.id = Math.abs(item.id);
	        }
	      });
	      return Arr;
	    },
	    resetSetting() {
	      this.configs = {
	        name: "",
	        legal: false,
	        shifts: ""
	      };
	      this.data = this.data.map((item, index) => {
	        return { ...item, _checked: false };
	      });
	      this.selectData = [];
	    },
	    singleSelect() {},
	    handleSelect(select) {
	      if (this.settingMode == "add") {
	        this.selectData = select;
	        console.log(this.data);
	        console.log(this.selectData);
	        this.compileDiffData = this.getArrDifference(
	          this.data,
	          this.selectData
	        );
	      } else {
	        this.selectData = select;
	        this.compileDiffData = this.getArrDifference(
	          this.data,
	          this.selectData
	        );
	        console.log("diff", this.compileDiffData);
	      }
	    },
	    emitCloseModal() {
	      this.resetSetting();
	      this.$emit("handleChildDisplay");
	    },
	
	    //修改班次
	    changeClasses() {
	      this.data[this.currentSelect].times = this.selectTimes;
	      this.data[this.currentSelect].id = this.selectID;
	      this.data[this.currentSelect]._checked = true;
	      this.showArrangeModal = false;
	      this.currentChoose = "";
	      this.currentSelect = -1;
	      this.selectID = -1;
	      this.selectTimes = "";
	    },
	
	    //提交考勤设置
	    Confirms() {
	      if (!this.configs.name) {
	        this.$Message.warning("请填写考勤组名称");
	        return false;
	      }
	      if (this.selectData.length == 0) {
	        this.$Message.warning("请选中工作日设置");
	        return false;
	      }
	      this.configs.legal = +this.configs.legal + "";
	
	      var reverseData;
	      //未选中数据id取反
	      if (this.compileDiffData.length > 0) {
	        reverseData = this.transDiff(this.compileDiffData);
	      } else {
	        reverseData = [];
	      }
	
	      //添加设置操作
	      if (this.settingMode === "add") {
	        let datas = [...reverseData, ...this.selectData];
	        datas.sort((a, b) => {
	          return a.index - b.index;
	        });
	        this.configs.shifts = datas.map(item => item.id).join(",");
	        this.addSetting(this.configs);
	      }
	
	      //编辑设置操作
	      if (this.settingMode === "edit") {
	        //no selected new data item, only compile selected data
	        if (this.compileDiffData.length < 1) {
	            if(this.selectData.length == this.data.length) {
48999cb9a08289eb796f81653c4b677f|fugitive:/Users/aA/Desktop/chibi/cb/.git//0/src/views/statistics/components/addAttencanceSetting.vue|424|1|V|vue
