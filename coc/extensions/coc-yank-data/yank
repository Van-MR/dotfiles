	    transformStates() {
	      if (
	        this.stat3 == "禁用" ||
	        this.stat2 == "禁用" ||
	        this.state == "禁用"
	      ) {
	        this.stat3 = 0;
	        this.stat2 = 0;
	        this.state = 0;
	      } else if (
	        this.stat3 == "启用" ||
	        this.stat2 == "启用" ||
	        this.state == "启用"
	      ) {
	        this.stat3 = 1;
	        this.stat2 = 1;
	        this.state = 1;
	      }
	    },
