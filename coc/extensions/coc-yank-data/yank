	    func SetTitle()
	        call setline(1,"/**") 
	        call append(line("."), " *   Copyright (C) ".strftime("%Y")." All rights reserved.")
	        call append(line(".")+1, " *") 
	        call append(line(".")+2, " *   FileName      ：".expand("%:t")) 
