	<template>
	  <div>
	    <div class="bread">
	      <p>
	        <Icon type="ios-people" size="18" class="icon-bread"></Icon> 访客管理 /
	        <Icon type="navicon" size="18" class="icon-bread"></Icon>
	        {{ $route.meta.title }}
	      </p>
	    </div>
	    <div class="list">
	      <Card
	        :style="{ marginTop: '15px', marginBottom: '20px', height: '80px' }"
	      >
	        <!-- 顶部搜索栏 -->
	        <Form
	          ref="searchroleform"
	          :style="{ marginTop: '5px', marginLeft: '-30px' }"
	          model="searchroleform"
	          :label-width="64"
	          inline
	        >
	          <Row>
	            <Col span="6" :style="{ paddingLeft: '18px' }">
	              <FormItem label="关键词">
	                <Input placeholder="请输入关键词搜索" v-model="keyword" />
	              </FormItem>
	            </Col>
	            <Col span="6" :style="{ paddingLeft: '18px' }">
	              <FormItem label="性别">
	                <Select v-model="sex" style="width:160px">
	                  <Option v-for="item in sexs" :key="item.id">{{
	                    item.name
	                  }}</Option>
	                </Select>
	              </FormItem>
	            </Col>
	            <Col span="6">
	              <FormItem label="添加时间">
	                <DatePicker
	                  type="datetime"
	                  placeholder="请选择"
	                  :value="times"
	                  @on-change="handleTimeChange"
	                  style="width:160px"
	                ></DatePicker>
	              </FormItem>
	            </Col>
	            <Col span="6" :style="{ position: 'relative', left: '45px' }">
	              <Button
	                type="primary"
	                icon="ios-search"
	                v-track:18015.click="getClassList"
	                :loading="searchLoading"
	                >搜索</Button
	              >
	              <Button
	                type="primary"
	                icon="ios-cloud-upload-outline"
	                v-track:18015.click="Export"
	                :loading="export_status"
	                >导出</Button
	              >
	            </Col>
	          </Row>
	        </Form>
	      </Card>
	      <Card>
	        <Row :style="{ marginBottom: '30px' }">
	          <h5 class="title-head">白名单</h5>
	          <Table
	            stripe
	            :columns="columns1"
	            :data="data1"
	            :loading="tableLoading"
	          ></Table>
	        </Row>
	        <Row :style="{ marginTop: '35px' }">
	          <Page
	            :style="{ float: 'right' }"
	            :total="this.totalCount"
	            :current="this.currentPage"
	            :page-size="this.pageSize"
	            size="small"
	            show-total
	            show-elevator
	            @on-change="changePage"
	          ></Page>
	        </Row>
	      </Card>
	    </div>
	  </div>
	</template>
	<script>
	import { mapState, mapActions } from "vuex";
	export default {
	  data() {
	    return {
	      tableLoading: false,
	      times: "",
	      keyword: "",
	      group: "",
	      sex: "",
	      sexs: [{ name: "男", id: 1 }, { name: "女", id: 0 }],
	      data1: [],
	      searchLoading: false,
	      export_status: false,
	      currentPage: 1,
	      pageSize: 8,
	      totalCount: 7,
	      columns1: [
	        {
	          title: "序号",
	          key: "id",
	          width: 60,
	          align: "center"
	        },
	        {
	          title: "照片",
