	    async (url, method = "GET", body = null, headers = {}) => {
	      setLoading(true);
	      try {
	        const responce = await fetch(url, { method, body, headers });
	        const data = await responce.json();
	        if (!responce.ok) {
	          throw new Error(data.message || "http rquest failed");
	        }
	        setLoading(false);
	        return data;
	      } catch (e) {
	        setLoading(false);
	        setError(e.message);
	        throw e;
	      }
	    }
