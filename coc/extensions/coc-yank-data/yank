	    bubuleEffect(i, name, avatar) {
	      var bubbles = document.getElementsByClassName("bubble");
	      var t = bubbles[i - 1].getElementsByClassName("name")[0];
	      t.innerHTML = name;
	      var a = bubbles[i - 1].getElementsByClassName("avatar")[0];
	      a.src = avatar;
	      bubbles[i - 1].style.display = "block";
	      if (i - 1 > 0) {
	        bubbles[i - 2].style.display = "none";
	      }
	    },
