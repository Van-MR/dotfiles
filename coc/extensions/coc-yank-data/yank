22767c2a3ee82b8ba113907cffd83930|file:///Users/aA/Desktop/chibi/cb/src/views/system/components/setting.vue|87|1|V|vue
	<script>
	import { mapState, mapActions } from "vuex";
	import { appRouter } from "../../../router/router.js";
	export default {
	  data() {
	    return {
	      currentMode: "",
	      modesList: [],
	      copyright: "",
	      imgSrc: "",
	      files: "",
	      file: "",
	      systemInfo: {
	        id: "",
	        name: "",
	        sensor: "",
	        logo: "",
	        copyright: ""
	      }
	    };
	  },
	  mounted() {
	    this.getSystemInfo();
	    this.getSceneModes();
	  },
	  computed: {
	    ...mapState({
	      menuList: state => state.app.menuList,
	      backendMenuList: state => state.app.backendMenuList
	    })
	  },
	  methods: {
	    ...mapActions(["getAccessMenuList"]),
	      fileChange(){
	          this.file = this.$refs['upload-input'].files[0]
	          console.log(this.file);
	          this.uploadLogo(this.file);
	      },
	      trigeFileUpload(){
	          this.$refs['upload-input'].dispatchEvent(new MouseEvent('click')) 
	      },
	    //获取场景字典
	    getSceneModes() {
	      this.$axios
	        .get(`${this.URL_PREFIX}/dict/sensor`)
	        .then(res => {
	          if (res.data.code === "0") {
	            this.modesList = res.data.result;
	          }
	        })
	        .catch(err => {
	          console.log(err);
	        });
	    },
	
	    //获取系统设置信息
	    getSystemInfo() {
	      this.$axios
	        .get(`${this.URL_PREFIX}/system/config`)
	        .then(res => {
	          if (res.data.code == "0") {
	            this.systemInfo = res.data.result[0];
	          }
	        })
	        .catch(err => {
	          console.log(err);
	        });
	    },
	    displayUpload() {
	      this.systemInfo.logo = "";
	    },
	    //上传logo图
	    uploadLogo(file) {
	      var oForm = new FormData();
	      //添加key
	      oForm.append("files", file);
	      //添加请求头
	      let config = {
	        headers: { "Content-Type": "multipart/form-data" }
	      };
	      //send request
	      this.$axios
	        .post(`${this.URL_PREFIX}/upload/pic`, oForm, config)
	        .then(res => {
	          if (res.data.code == "0") {
	            this.$Message.success("上传成功");
	            this.systemInfo.logo = res.data.url;
	            this.file = "";
	          } else {
	            this.$Message.error("上传失败!");
	            this.systemInfo.logo = "";
	            this.file = "";
	          }
	        })
	        .catch(error => {
	          this.$Message.error("服务器出错...");
	          this.systemInfo.logo = "";
	          this.file = "";
	          console.log(error);
	        });
	    },
	    handleUpload(file) {
	      //获取上传文件信息
	      this.file = file;
	      console.log(file);
	      //发送请求
	      this.uploadLogo();
	      return false;
	    },
	    //设置系统设置信息
	    confirmSetting() {
	      console.log(this.systemInfo.id);
	      this.$axios
	        .post(`${this.URL_PREFIX}/system/config`, this.systemInfo)
	        .then(res => {
	          if (res.data.code == "0") {
	            this.$store.commit("updateTitle", this.systemInfo.name);
	            window.document.title = this.systemInfo.name;
	            this.$store.commit("updateCopyRight", this.systemInfo.copyright);
	            this.$store.commit("updateSystemName", this.systemInfo.name);
	            this.$store.commit("updateLogoURI", this.systemInfo.logo);
	            this.getSystemInfo();
	
	            this.getAccessMenuList();
	            let MenuListNames = this.backendMenuList.map(
	              menuItem => menuItem.name
	            );
	            //权限菜单
	            let acccess_MenuList = [];
	            MenuListNames.forEach(meunName => {
	              appRouter.filter(item => {
	                if (item.children[0].title == meunName) {
	                  acccess_MenuList.push(item);
	                }
	              });
	            });
	            this.$store.commit("updateMenulist", acccess_MenuList);
	
	            this.$Message.success("系统配置成功");
	          } else {
	            this.$Message.error("系统配置失败");
	          }
	        })
	        .catch(err => {
	          console.log(err);
	        });
	    },
	    //重置系统信息 清空
	    resetSetting() {
	      this.systemInfo = {
	        name: "",
	        logo: "",
	        sensor: "",
	        copyright: ""
	      };
	    }
	  }
	};
	</script>
