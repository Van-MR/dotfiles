	        .get(
	          `${this.URL_PREFIX}/attendance/group/ctrl?id=&group=${group}&page=${this.currentPage1}&size=${this.pageSize1}`
	        )
	        .then(res => {
	          if (res.data.code === "0") {
	            this.showSubjectInfoModal = true;
	            this.totalCount1 = res.data.result[0].total;
	            const { info, header, table_header } = res.data.result[0];
	            this.groupInfos = header;
	            this.subjectData = info;
	
	            this.generateColumns(this.subjectColumns, table_header);
	            this.subjectTitle = res.data.table_name;
	            this.renderSubjectCloumns = true;
	          }
	        })
	        .catch(err => {
	          this.$Message.error("服务器出错!");
	          console.log(err);
	        });
	    }
