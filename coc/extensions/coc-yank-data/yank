	<script>
	import addAccess from "./addAccess";
	export default {
	  name: "trafficAccess",
	  components: { addAccess },
	  data() {
	    return {
	      showAddComponent: false,
	      tableLoading: false,
	      searchLoading: false,
	      export_status: false,
	      status: [{ id: 0, name: "使用中" }, { id: 1, name: "禁用" }],
	      query: {
	        keyword: "",
	        statu: "",
	        times: "",
	        currentPage: 1,
	        pageSize: 8
	      },
	      settingMode: "add",
	      settingConfigs: {
	        name: "",
	        times: "",
	        device_id: "",
	        group: false
	      },
	      data1: [],
	      totalCount: 3,
	      columns1: [
	        {
	          title: "序号",
	          key: "id",
	          width: 60,
	          align: "center"
	        },
	        {
	          title: "名称",
	          key: "name",
	          align: "center"
	        },
	        {
	          title: "添加时间",
	          key: "posttime",
	          align: "center"
	        },
	        {
	          title: "状态",
	          key: "status",
	          align: "center",
	          render: (h, params) => {
	            let text = params.row.status;
	            let style = {
	              color: ""
	            };
	            if (text == "启用") {
	              style.color = "#228824 ";
	            } else {
	              style.color = "#D0646A";
	            }
	            return h("div", [
	              h(
	                "p",
	                {
	                  style: style,
	                  props: {
	                    type: "text"
	                  }
	                },
	                text
	              )
	            ]);
	          }
	        },
	        {
	          title: "开放时间",
	          key: "times",
	          align: "center"
	        },{
	          title: "操作",
	          align: "center",
	          render: (h, params) => {
	            return h("div", [
	              h(
	                "p",
	                {
	                  style: {
	                    display: "inline-block",
	                    marginRight: "15px",
	                    color: "#258ee9",
	                    cursor: "pointer"
	                  },
	                  on: {
	                    click: () => {
	                      this.getAttenceInfo(params.row.id);
	                    }
	                  }
	                },
	                "编辑"
	              ),
	              h(
	                "p",
	                {
	                  style: {
	                    display: "inline-block",
	                    marginRight: "5px",
	                    color: "#ed462f",
	                    cursor: "pointer"
	                  },
	                  on: {
	                    click: () => {
	                      this.$Modal.confirm({
	                        title: "提示",
	                        content: "<h3>确定要删除吗?</h3>",
	                        onOk: () => {
	                          this.$axios
	                            .delete(`${this.URL_PREFIX}/statistic/attend`, {
	                              data: {
	                                id: params.row.id + ""
	                              }
	                            })
	                            .then(res => {
	                              if (res.data.code == "0") {
	                                setTimeout(() => {
	                                  this.$Message.success("删除成功");
	                                  this.getSettingList();
	                                }, 500);
	                              } else {
	                                setTimeout(() => {
	                                  this.$Message.error("删除失败");
	                                }, 500);
	                              }
	                            })
	                            .catch(error => {
	                              this.$Message.error("服务器出错");
	                              console.log(error);
	                            });
	                        }
	                      });
	                    }
	                  }
	                },
	                "删除"
	              )
	            ]);
	          }
	        }
	        {
	          title: "操作",
	          align: "center",
	          render: (h, params) => {
	            return h("div", [
	              h(
	                "p",
	                {
	                  style: {
	                    color: "#258ee9",
	                    cursor: "pointer"
	                  },
	                  on: {
	                    click: () => {
	                      this.$router.push({
	                        path: `/attendance/profile/${params.row.id}`,
	                        query: {
	                          times: this.times
	                        }
	                      });
	                    }
	                  }
	                },
	                "查看详情"
	              )
	            ]);
	          }
	        }
	      ],
	      data1: [
	        {
	          id: 1,
	          name: "商务部权限",
	          open: "全天",
	          status: "使用中",
	          time: "2019-02-20 10:30:29"
	        },
	        {
	          id: 2,
	          name: "研发部权限",
	          open: "全天",
	          status: "禁用",
	          time: "2019-02-20 10:30:29"
	        },
	        {
	          id: 3,
	          name: "财务部权限",
	          open: "全天",
	          status: "使用中",
	          time: "2019-02-20 10:30:29"
	        },
	        {
	          id: 4,
	          name: "其它",
	          open: "全天",
	          status: "使用中",
	          time: "2019-02-20 10:30:29"
	        }
	      ]
	    };
	  },
	  mounted() {
	    this.getAccessList();
	  },
	  methods: {
	    //通行权限列表
	    getAccessList() {
	      this.tableLoading = true;
	      this.$axios
	        .get(
	          `${this.URL_PREFIX}/current/access?page=${this.query.currentPage}&size=${this.query.pageSize}&keyword=${this.query.keyword}&status=${this.query.statu}&times=${this.query.times}`
	        )
	        .then(res => {
	          if (res.data.code == "0") {
	            this.tableLoading = false;
	            this.data1 = res.data.result;
	            this.totalCount = res.data.total;
	          } else {
	            this.tableLoading = false;
	            this.data1 = [];
	            this.$Message.warning("暂无搜索结果,请重新搜索");
	            return false;
	          }
	        })
	        .catch(err => {
	          this.tableLoading = false;
	          this.$Message.error("服务器出错...");
	          console.log(err);
	        });
	    },
	
	    //通行权限详情
	    getAttenceInfo(id) {
	      this.$axios
	        .get(`${this.URL_PREFIX}/current/access/info?id=${id}`)
	        .then(res => {
	          if (res.data.code === "0") {
	            console.log(res.data.result);
	            const { id, name, attend, status, legal } = res.data.result[0];
	            this.settingConfigs.id = id;
	            this.settingConfigs.name = name;
	            this.settingConfigs.legal = legal === "1" ? true : false;
	            this.settingConfigs.shifts = attend;
	            this.settingMode = "edit";
	            this.showAddComponent = true;
	          }
	        })
	        .catch(err => {
	          this.$Message.error("服务器出错!");
	          console.log(err);
	        });
	    },
	
	    //通行权限编辑
	    compileSetting(id, settingConfig) {
	      this.$axios
	        .put(`${this.URL_PREFIX}/current/access`, { id, ...settingConfig })
	        .then(res => {
	          if (res.data.code == "0") {
	            this.$Message.success("编辑成功");
	            this.showAddComponent = false;
	            this.settingMode = "add";
	            this.getAccessList();
	          } else {
	            this.$Message.error(res.data.msg);
	          }
	        })
	        .catch(err => {
	          this.$Message.error("服务器出错!");
	          console.log(err);
	        });
	    },
	
	    //通行权限添加
	    addSetting(settingConfig) {
	      this.$axios
	        .post(`${this.URL_PREFIX}/current/access`, settingConfig)
	        .then(res => {
	          if (res.data.code === "0") {
	            this.$Message.success("添加成功");
	            this.showAddComponent = false;
	            this.getAccessList();
	          }
	        })
	        .catch(err => {
	          this.$Message.error("服务器出错!");
	          console.log(err);
	        });
	    },
	
	    //close child component
	    handleChildDisplay() {
	      this.showAddComponent = false;
	    },
	
	    //handle pagination
	    changePage(index) {
	      this.query.currentPage = index;
	      this.getAccessList();
	    },
	
	    //获取日期选择器的起始时间
	    handleTimeChange(datetime) {
	      this.query.times = datetime;
	    }
	  }
	};
	</script>
