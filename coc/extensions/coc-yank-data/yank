9da72aa99d172b9ca9a66c1e07782ac5|file:///Users/aA/Desktop/chibi/cb/src/views/home/components/visiting.vue|1|1|V|vue
	<template>
	  <div :class="className" :style="{ height: height, width: width}"></div>
	</template>
	
	<script>
	import echarts from "echarts";
	require("echarts/theme/macarons"); // echarts theme
	
	const animationDuration = 1500;
	const option = {
	  tooltip: {
	    trigger: "item",
	    formatter: "{a} <br/>{b} : {c} ({d}%)"
	  },
	  color: ["#8fc31f", "#f35833", "#00ccff", "#ffcc00"],
	  series: [
	    {
	      name: "签到比例分析",
	      type: "pie",
	      radius: "85%",
	      center: ["40%", "65%"],
	      data: [
	        { value: 335, name: "准时" },
	        { value: 310, name: "迟到" },
	        { value: 234, name: "请假" },
	        { value: 135, name: "未到" }
	      ],
	      itemStyle: {
	        emphasis: {
	          shadowBlur: 10,
	          shadowOffsetX: 0,
	          shadowColor: "rgba(0, 0, 0, 0.5)"
	        }
	      },
	      itemStyle: {
	        normal: {
	          label: {
	            show: true,
	            //	                            position:'inside',
	            formatter: "{b} : {c} ({d}%)"
	          }
	        },
	        labelLine: { show: true }
	      }
	    }
	  ]
	};
	
	export default {
	  name: "visiting",
	  props: {
	    chartData: {
	      type: Array,
	      require: true
	    },
	    className: {
	      type: String,
	      default: "line"
	    },
	    width: {
	      type: String,
	      default: "100%"
	    },
	    height: {
	      type: String,
	      default: "270px"
	    }
	  },
	  data() {
	    return {
	      chart: null
	    };
	  },
	  watch: {
	    chartData: {
	      deep: true,
	      handler(val) {
	        this.setOptions(val);
	      }
	    }
	  },
	  beforeDestroy() {
	    if (!this.chart) {
	      return;
	    }
	    this.chart.dispose();
	    this.chart = null;
	  },
	  mounted() {
	    console.log(this.chartData);
	    this.initChart();
	  },
	  methods: {
	    setOptions() {
	      this.chart.setOption({
	        tooltip: {
	          trigger: "item",
	          formatter: "{a} <br/>{b} : {c}人 ({d}%)"
	        },
	        color: ["#61d6fc", "#eb806a"],
	        series: [
	          {
	            name: "今日访客状态",
	            type: "pie",
	            radius: "60%",
	            center: ["50%", "48%"],
	            data: this.chartData,
	            itemStyle: {
	              emphasis: {
	                shadowBlur: 10,
	                shadowOffsetX: 0,
	                shadowColor: "rgba(0, 0, 0, 0.5)"
	              }
	            },
	            itemStyle: {
	              normal: {
	                label: {
	                  show: true,
	                  formatter: "{b} : {c}人 ({d}%)"
	                }
	              },
	              labelLine: { show: true }
	            }
	          }
	        ]
	      });
	    },
	    initChart() {
	      this.chart = echarts.init(this.$el, "macarons");
	      this.setOptions(this.chartData);
	      var _this = this;
	      window.onresize = function() {
	        _this.chart.resize();
	      };
	      window.onload = function() {
	        _this.chart.resize();
	      };
	    }
	  }
	};
	</script>
