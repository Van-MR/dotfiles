	                      this.$Modal.confirm({
	                        title: "提示",
	                        content: "<h3>确定要删除吗?</h3>",
	                        onOk: () => {
	                          this.$axios
	                            .delete(`${this.URL_PREFIX}/user`, {
	                              headers: { "Content-Type": "application/json" },
	                              data: {
	                                id: params.row.id + ""
	                              }
	                            })
	                            .then(res => {
	                              if (res.data.code == "0") {
	                                setTimeout(() => {
	                                  this.getUserList();
	                                  this.$Message.success("删除成功");
	                                }, 500);
	                              } else if (res.data.code == "10003") {
	                                setTimeout(() => {
	                                  this.$Message.warning("删除的用户不存在");
	                                }, 500);
	                              } else {
	                                setTimeout(() => {
	                                  this.$Message.error("删除失败");
	                                }, 500);
	                              }
	                            })
	                            .catch(error => {
	                              console.log(error);
	                            });
	                        }
	                      });
