	  //获取uri重定向地址中的code
	  const getParams = () => {
	    let code;
	    let location = window.location.href;
	    if (location.includes("code=")) {
	      let k = location.split("?")[1];
	      let d = k.split("&")[0];
	      let f = d.substr(0, 5);
	      code = d.replace(f, "");
	      return code;
	    }
	
	    code = "";
	    return code;
	  };
	
