ed4aa33ef0fd60e33ba249c6db1d2e15|file:///Users/aA/Desktop/face_mobile/src/pages/Visitor/index.js|68|1|V|javascript
	    // 获取部门词典表
	    const getPurposeDict = async () => {
	        try {
	            const res = await fetch(`${baseURL}/common/dict/group?type=1`)
	            const jsonData = await res.json()
	            if (jsonData.code === '0') {
	                setDeps([{name: '请选择', id: '-1'}, ...jsonData.result])
	                return
	            }
	            setDeps([{name: '请选择', id: -1}])
	        } catch (e) {
	            /* handle error */
	            setDeps([{name: '请选择', id: -1}])
	            console.log(e)
	        }
	    }
	
	    //获取来访目的词典
	    const getDepDict = async () => {
	        try {
	            const res = await fetch(`${baseURL}/common/dict/purpose`)
	            const jsonData = await res.json()
	            if (jsonData.code === '0') {
	                setResonOptions([{name: '请选择', id: '-1'}, ...jsonData.result])
	                return
	            }
	            setResonOptions([{name: '请选择', id: -1}])
	        } catch (e) {
	            /* handle error */
	            setResonOptions([{name: '请选择', id: -1}])
	            console.log(e)
	        }
	    }
	
