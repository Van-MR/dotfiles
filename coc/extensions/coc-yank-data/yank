13fadace1a9f8662879c58be3f09f6c3|file:///Users/aA/Desktop/chibi/cb/src/views/cam/components/faceLabel.vue|71|1|V|vue
	  components: { optionTool },
	  data() {
	    return {
	      type: "0",
	      currentIP: "",
	      showMenuList: false,
	      channelList: [],
	      currentIndex: 0,
	      faceLogs: [],
	      shouldReforceUpdate: true,
	      offsetIndex: 0,
	      timer: null,
	      countTime: 0,
	      noPasser: false,
	      playerOptions: {
	        sources: [
	          {
	            type: "rtmp/mp4",
	            src: ""
	          }
	        ],
	        techOrder: ["flash"],
	        flash: {
	          swf: require("../../static/swf/video-js.swf")
	        },
	        autoplay: true,
	        controls: false,
	        poster: require("../../assets/images/camposter.jpg")
	      }
	    };
	  },
	  created() {
	    this.$store.commit("toggleIconMenu");
	  },
	  mounted() {
	    this.getChanels();
	    this.initWebSocket();
	    window.onbeforeunload = e => this.beforeunloadHandler(e);
	  },
	
	  watch: {
	    //30s内无新抓拍数据返回,清空抓拍列表
	    countTime(val) {
	      if (this.countTime === 30) {
	        this.faceLogs = [];
	        this.closeTimer();
	      }
	    }
	  },
	  methods: {
	    //切换不同失败场景 人脸 抽烟 安全帽
	    changeAlgorithmType(type) {
	      this.type = type;
	      this.loadAlgorithm();
	    },
	
	    //加载算法
	    loadAlgorithm(ip) {
	      this.$axios
	        .post(`${this.URL_PREFIX}/device/video/label`, {
	          ip: this.currentIP,
	          type: this.type
	        })
	        .then(res => {
	          if (res.data.code === "0") {
	            this.$Message.success(res.data.msg);
	          } else {
	            this.$Message.error(res.data.msg);
	          }
	        })
	        .catch(err => {
	          this.$Message.error("服务器出出错...");
	          console.log(err);
	        });
	    },
	
	    //视频推流
	    fetchUpstream(ip) {
	      return new Promise((resolve, reject) => {
	        this.$axios
	          .post(`${this.URL_PREFIX}/device/video/rtmp`, {
	            ip
	          })
	          .then(res => {
	            if (res.data.code === "0") {
	              resolve(res.data.url);
	            }
	          })
	          .catch(err => {
	            reject(err);
	            this.$Message.error("服务器出错");
	          });
	      });
	    },
	
	    //获取频道列表
	    getChanels() {
	      this.$axios
	        .get(`${this.URL_PREFIX}/device/video/list?keyword=`)
	        .then(res => {
	          if (res.data.code === "0") {
	            this.channelList = res.data.result;
	            this.currentIP = this.channelList[0].ip;
	            this.loadAlgorithm(this.currentIP);
	            this.fetchUpstream(this.currentIP).then(url => {
	              this.playerOptions.sources[0].src = url;
	            });
	          }
	        })
	        .catch(err => {
	          console.log(err);
	        });
	    },
	
	    //切换通道
	    switchCam(event, item, index) {
	      this.currentIP = item.ip;
	      this.currentIndex = index;
	      this.loadAlgorithm(this.currentIP);
	      this.fetchUpstream(this.currentIP).then(url => {
	        this.playerOptions.sources[0].src = url;
	      });
	    },
	
	    initWebSocket() {
	      const wsurl = `${this.URL_WS1}`;
	      this.websock = new WebSocket(wsurl);
	      this.websock.onopen = this.websocketopen;
	      this.websock.onmessage = this.websocketonmessage;
	      this.websock.onclose = this.websocketclose;
	    },
	
	    beforeunloadHandler(e) {
	      this.websock.send("close");
	    },
	
	    loadTimer() {
	      this.timer = setInterval(() => {
	        this.countTime++;
	      }, 1000);
	    },
	
	    closeTimer() {
	      clearInterval(this.timer);
	      this.countTime = 0;
	    },
	
	
	  },
	
