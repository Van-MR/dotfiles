cb107121ad01e49a44af1e4c4ba4d774|file:///Users/aA/Desktop/chibi/cb/src/views/traffic/components/trafficEvent.vue|1|1|V|vue
	<template>
	  <div>hello world</div>
	</template>
	<script>
	import { mapState, mapActions } from "vuex";
	export default {
	  data() {
	    return {
	      tableLoading: false,
	      searchLoading: false,
	      export_status: false,
	      showTrafficInfo: false,
	      directions: [
	        { id: 0, name: "进门" },
	        { id: 1, name: "出门" }
	      ],
	      trafficInfo: {},
	      query: {
	        keyword: "",
	        direction: "",
	        times: "",
	        currentPage: 1,
	        pageSize: 8
	      },
	      totalCount: 0,
	      data1: [],
	      columns1: [
	        {
	          title: "序号",
	          key: "id",
	          width: 80,
	          align: "center"
	        },
	        {
	          title: "照片",
	          key: "pic",
	          width: 100,
	          align: "center",
	          render: (h, params) => {
	            let src = params.row.pic;
	            return h("div", [
	              h("img", {
	                attrs: {
	                  src
	                },
	                style: {
	                  width: "45px",
	                  height: "45px",
	                  cursor: "pointer",
	                  marginTop: "5px"
	                }
	              })
	            ]);
	          }
	        },
	        {
	          title: "姓名",
	          key: "name",
	          align: "center"
	        },
	        {
	          title: "身份",
	          key: "type",
	          align: "center"
	        },
	        {
	          title: "设备名称",
	          key: "device",
	          align: "center"
	        },
	        {
	          title: "进出方向",
	          key: "direction",
	          align: "center"
	        },
	        {
	          title: "通行时间",
	          key: "times",
	          align: "center"
	        },
	        {
	          title: "操作",
	          align: "center",
	          render: (h, params) => {
	            return h("div", [
	              h(
	                "p",
	                {
	                  style: {
	                    color: "#258ee9",
	                    cursor: "pointer"
	                  },
	                  on: {
	                    click: () => {
	                      this.getTrafficInfo(params.row.id);
	                    }
	                  }
	                },
	                "查看详情"
	              )
	            ]);
	          }
	        }
	      ],
	      data1: [
	        {
	          id: 1,
	          pic: "",
	          name: "李文",
	          identity: "员工",
	          equip: "门禁机",
	          direction: "西南",
	          time: "2019-02-20 10:30:29"
	        },
	        {
	          id: 2,
	          pic: "",
	          name: "张华",
	          identity: "员工",
	          equip: "大门",
	          direction: "西南",
	          time: "2019-02-20 10:30:29"
	        },
	        {
	          id: 2,
	          pic: "",
	          name: "王条",
	          identity: "员工",
	          equip: "门禁机",
	          direction: "西南",
	          time: "2019-02-20 10:30:29"
	        }
	      ]
	    };
	  },
	  mounted() {
	    //this.getTrafficList();
	  },
	  methods: {
	    getTrafficList() {
	      this.tableLoading = true;
	      this.$axios
	        .get(
	          `${this.URL_PREFIX}/current/log?page=${this.query.currentPage}&size=${
	            this.query.pageSize
	          }&keyword=${this.query.keyword || ""}&direction=${this.query
	            .direction || ""}&times=${this.query.times || ""}`
	        )
	        .then(res => {
	          if (res.data.code == "0") {
	            this.tableLoading = false;
	            this.data1 = res.data.result;
	            this.totalCount = res.data.total;
	          } else {
	            this.tableLoading = false;
	            this.data1 = [];
	            this.$Message.warning("暂无搜索结果,请重新搜索");
	            return false;
	          }
	        })
	        .catch(err => {
	          this.tableLoading = false;
	          this.$Message.error("服务器出错...");
	          console.log(err);
	        });
	    },
	
	    //trafficInfo
	    getTrafficInfo(id) {
	      this.$axios
	        .get(`${this.URL_PREFIX}/current/log/info?id=${id}`)
	        .then(res => {
	          if (res.data.code === "0") {
	            this.showTrafficInfo = true;
	            const {
	              name,
	              pic,
	              device,
	              group,
	              direction,
	              type,
	              times,
	              status
	            } = res.data.result[0];
	
	            this.trafficInfo.name = name;
	            this.trafficInfo.pic = JSON.parse(pic);
	            this.trafficInfo.device = device;
	            this.trafficInfo.direction = direction;
	            this.trafficInfo.group = group;
	            this.trafficInfo.type = type;
	            this.trafficInfo.times = times;
	            this.trafficInfo.status = status;
	          } else {
	            this.$Message.error(res.data.message);
	          }
	        })
	        .catch(err => {
	          this.$Message.error("服务器出错!");
	          console.log(err);
	        });
	    },
	    //表格数据导出
	    Export() {
	      this.export_status = true;
	      this.$axios
	        .get(
	          `${this.URL_PREFIX}/current/log/export?keyword=${this.query.keyword}&direction=${this.query.direction}&times=${this.query.times}`
	        )
	        .then(res => {
	          if (res.data.code === "0") {
	            this.export_status = false;
	            this.$Message.success("导出成功");
	            setTimeout(() => {
	              window.location.href = `${res.data.url}`;
	            }, 500);
	          } else {
	            this.export_status = false;
	            this.$Message.error("导出失败");
	          }
	        })
	        .catch(err => {
	          this.$Message.error("服务器出错!");
	          this.export_status = false;
	          console.log(err);
	        });
	    },
	
	    //分页处理
	    changePage(index) {
	      this.query.currentPage = index;
	      this.getTrafficList();
	    },
	
	    //获取日期选择器的起始时间
	    handleTimeChange(datetime) {
	      this.query.times = datetime;
	    }
	  }
	};
	    }
	  }
	};
	
	.list {
	  padding: 10px 20px;
	  height: calc(100vh - 102px);
	  overflow-y: scroll;
	  .title {
	    padding: 14px;
	    border-bottom: 1px solid #d9d9d9;
	  }
	}
	.formItem {
	  margin: 10px 0;
	  .label {
	    font-weight: 600;
	    margin: 7px 0;
	  }
	  .active {
	    color: "#68bd7a";
	  }
	}
	    color: "#68bd7a";
	  }
	}
	</style>
