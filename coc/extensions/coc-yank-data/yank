95f24148299ae882b630bf0b017dc881|file:///Users/aA/Desktop/electric-drawing/js/main.js|1|1|V|javascript
	window.onload = function() {
	  main();
	};
	
	function main() {
	  let scene, camera, renderer;
	
	  scene = new THREE.Scene();
	
	  let canvas = document.querySelector("#c");
	  let width = canvas.clientWidth;
	  let height = canvas.clientHeight;
	  renderer = new THREE.WebGLRenderer({ canvas });
	
	  let fov = 75;
	  let aspect = width / height;
	  let near = 0.1;
	  let far = 1000;
	  camera = new THREE.PerspectiveCamera(fov, aspect, near, far);
	  camera.position.z = 5;
	  camera.lookAt(new THREE.Vector3(0, 0, 0));
	
	  let sceneLight = new THREE.AmbientLight(0xffffff);
	  scene.add(sceneLight);
	  let color = new THREE.Color(0x444444);
	  let intensity = 1;
	  let light = new THREE.DirectionalLight(color, intensity);
	  light.position.set(-1, 3, 2);
	  scene.add(light);
	
	  let loader = new THREE.TextureLoader();
	  let texture = loader.load("../assets/images/bg.jpg");
	  console.log(texture);
	  scene.background = texture;
	
	  let cubeGeo = new THREE.BoxGeometry(2, 2, 2);
	  let cubeMat = new THREE.MeshPhongMaterial({ color: "red", map: texture });
	  let cube = new THREE.Mesh(cubeGeo, cubeMat);
	  scene.add(cube);
	
	  render();
	
	  function render() {
	    renderer.render(scene, camera);
	  }
	}
