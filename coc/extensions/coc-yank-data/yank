	        { name: "已离开", value: 80 },
	        { name: "正在访问", value: 120 }
	      ],
	      //通行趋势 折线图
	      lineChartData: {
	        xData: [],
	        expectedData: [],
	        legendData: []
	      }
	    };
	  },
	  methods: {
	    getStastiticInfo() {
	      this.$axios
	        .get(`${this.URL_PREFIX}/index/index`)
	        .then(res => {
	          if (res.data.code === "0") {
	            const { pass, equipment, trend, visitor } = res.data.result[0];
	            this.pass = pass[0];
	            this.equipment = equipment[0];
	            this.pieChartData = visitor;
	            this.lineChartData.xData = trend[0].times;
	            this.lineChartData.expectedData = trend[0].info;
	            this.lineChartData.legendData = trend[0].type;
	          }
	        })
	        .catch(err => {
	          console.log(err);
	        });
	    }
	  },
	  created() {
	    //this.getStastiticInfo();
	  },
	  mounted() {}
	};
	</script>
