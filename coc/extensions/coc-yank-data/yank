5ca83bf59f889042a6a8638d9b5c709f|file:///Users/aA/Desktop/epidemicPrevention/src/views/qrcode/list.vue|372|1|V|vue
	    //导入文件类型验证
	    checkFileType(file) {
	      const fileExt = file.name.split(".")[1];
	      const AllowEtxs = ["xls"];
	      const isAllow = AllowEtxs.includes(fileExt);
	      if (!isAllow) {
	        this.file = "";
	        this.showUploadModal = false;
	        this.$Message.error("请选择合适的文件类型进行上传,如xxx.xls");
	      }
	    },
