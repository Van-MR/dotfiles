	    websocketclose() {
	      this.websock.send("close");
	    },
	
	    //数据接收
	    websocketonmessage(e) {
	      let res = JSON.parse(e.data);
	      let result = res.result;
	      this.websock.send("start");
	      var n = 0;
	      if (result.length > 0) {
	        this.currentNumber++;
	        this.offsetIndex++;
	        if (this.currentNumber === 5) {
	          this.currentNumber = 1;
	        }
	        let { name, age, type, sex, posttime, group, pic } = res.result[0];
	        //this.bubuleEffect(this.currentNumber, name, pic[0]);
	        let logItem = { name, age, sex, posttime, group, type, pic: pic[0] };
	        console.log(logItem);
