	        scene.add(light);
	
	        const boxWidth = 1;
	        const boxHeight = 1;
	        const boxDepth = 1;
	        const geometry = new THREE.BoxGeometry(boxWidth, boxHeight, boxDepth);
	
	        function makeInstance(geometry, x, color) {
	          const material = new THREE.MeshPhongMaterial({
	            color,
	            wireframe: true
	          });
	          const cube = new THREE.Mesh(geometry, material);
	          scene.add(cube);
	          cube.position.x = x;
	          return cube;
	        }
	
	        const cubes = [
