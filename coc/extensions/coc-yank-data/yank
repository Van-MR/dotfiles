	  //从支付宝端扫码 获取auth_code
	  const getCodeFromAliPay = () => {
	    var code = getParamsFromAliPay();
	
	    if (id && code) {
	      id = id.split("&")[0];
	      getQrCodeInfo(id);
	    }
	
	    //如果是支付宝进入，根据auth_code获取openId , 否则重定向到授权页面
	    fetch(`${BASE_URL}/phone/alipay/access?code=${code}`)
	      .then((res) => res.json())
	      .then((data) => {
	        if (data.code === "0") {
	          setToken(data.token);
	          localStorage.setItem("token", data.token);
	          setOnlyID(data.only_id);
	          fetchUserInfo();
	        } else if (data.code === "-10011") {
	          window.location.replace(
	            "https://openauth.alipay.com/oauth2/publicAppAuthorize.htm?app_id=" +
	              data.only_id +
	              "&scope=auth_user" +
	              "&redirect_uri=" +
	              window.location.href
	          );
	        }
	      })
	      .catch((e) => {
	        console.log(e);
	      });
	  };
	
	  //从微信端扫码 获取code
	  const getCode = useCallback(() => {
	    var code = getParams();
	    if (code) {
	      id = id.split("&")[0];
	    }
	
	    getOpenId(code);
	
	    if (id && code) {
	      getQrCodeInfo(id);
	      fetchUserInfo();
	    }
	  }, [fetchUserInfo, getOpenId, getQrCodeInfo]);
	
	  const getOpenId = async (code) => {
	    try {
	      const res = await fetch(`${BASE_URL}/phone/wechat/access?code=${code}`);
	      const jsonData = await res.json();
	      if (jsonData.code === "0") {
	        localStorage.setItem("openId", JSON.stringify(jsonData.only_id));
	        setToken(jsonData.token);
	        setOnlyID(jsonData.only_id);
	      } else if (jsonData.code === "-10011") {
	        localStorage.removeItem("openId");
	        //没有openId 重定向到授权页面
	        window.location.replace(
	          "https://open.weixin.qq.com/connect/oauth2/authorize?appid=" +
	            jsonData.only_id +
	            "&redirect_uri=" +
	            encodeURIComponent(window.location.href) +
	            "&response_type=code&scope=snsapi_userinfo&state=STATE#wechat_redirect"
	        );
	      }
	    } catch (e) {
	      console.log(e);
	    }
	  };
	
