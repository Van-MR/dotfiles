d7e419563b55a05b7b0dd1fd761b039a|file:///Users/aA/Desktop/highorderFc.js|3|1|V|javascript
	import axios from "axios";
	
	// custom  user component
	const User = ({user}) => {
	    return (
	        <div>
	            <strong>
	                {user.name} {user.username}
	            </strong>
	
	            <span> {user.email}</span>
	        </div>
	    );
	};
	
	const withUserIdComponent = Component => {
	    const newComponent = ({userId, ...props}) => {
	        <div>
	            const [user, setUser] = useState(null);
	        useEffect(() => {
	                axios
	                    .get(`http;//jsonplaceholder/users/${userId}`)
	                    .then(res => {
	                        setUser(res.data);
	                    })
	                    .catch(err => {
	                        console.log(err);
	                    });
	        }, []);
	
	        if (!user) {
	            return "loading...";
	        }
	        return <Component {...props} user={user} />;
	        </div>
	    };
	
	    return newComponent;
	};
	
	const App = () => {
	    const SpecificUser = withUserIdComponent(User);
	    return (
	        <div class="app">
	            <SpecificUser userId={1} />
	        </div>
	};
	
	const App = () => {
	    const SpecificUser = withUserIdComponent(User);
	    return (
	        <div class="app">
	            <SpecificUser userId={1} />
	        </div>
	    );
	};
	
	reactDom.render(<App />, document.getElementById("root"));
