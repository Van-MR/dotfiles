	    getAttendanceDict() {
	      this.$axios
	        .get(`${this.URL_PREFIX}/statistic/attend/dict`)
	        .then(res => {
	          if (res.data.code === "0") {
	            this.attendances = res.data.result;
	          } else {
	            this.attendances = [];
	          }
	        })
	        .catch(err => {
	          console.log(err);
	        });
	    },
