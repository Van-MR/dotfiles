	    getLogsList() {
	      this.tableLoading = true;
	      this.$axios
	        .get(
	          `${this.URL_PREFIX}/system/log?page=${this.currentPage}&size=${this.pageSize}&keyword=${this.keyword}&type=${this.type}&times=${this.times}`
	        )
	        .then(res => {
	          if (res.data.code === "0") {
	            this.tableLoading = false;
	            this.data1 = res.data.result;
	            this.totalCount = res.data.total;
	          }
	        })
	        .catch(err => {
	          this.tableLoading = false;
	          this.$Message.error("服务器出错!");
	          console.log(err);
	        });
	    },
	
