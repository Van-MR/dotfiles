	      return new Promise((resolve, reject) => {
	        this.$axios
	          .post(`${this.URL_PREFIX}/camera/rtmp`, {
	            ip
	          })
	          .then(res => {
	            if (res.data.code === "0") {
	              resolve(res.data.url);
	            }
	          })
	          .catch(err => {
	            reject(err);
	            this.$Message.error("服务器出错");
	          });
	      });
	    },
