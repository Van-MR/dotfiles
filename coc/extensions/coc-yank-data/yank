	    //文件导入
	    Import() {
	      let fForm = new FormData();
	      fForm.append("files", this.file);
	      let config = {
	        headers: {
	          "Content-Type": "multipart/form-data"
	        }
	      };
	      this.$axios
	        .post(`${this.URL_PREFIX}/device/export`, fForm, config)
	        .then(res => {
	          if (res.data.code === "0") {
	            this.showUploadModal = false;
	            this.file = "";
	            this.$Message.success("导入成功");
	            this.getQrCodeList();
	          }
	        })
	        .catch(err => {
	          this.showUploadModal = false;
	          this.file = "";
	          console.log(err);
	          this.$Message.error("服务器出错...");
