738d01a5f4b054f0e9bedcd7ae24d95d|file:///Users/aA/Desktop/epidemic-prevention/src/pages/Entry.js|96|1|V|javascript
	  const getOpenId = async (code) => {
	    try {
	      const res = await fetch(`${BASE_URL}/wx/user/access?code=${code}`);
	      const jsonData = await res.json();
	      if (jsonData.code === "0") {
	        localStorage.setItem("openId", JSON.stringify(jsonData.only_id));
	        setToken(jsonData.token);
	      } else if (jsonData.code === "-10011") {
	        localStorage.removeItem("openId");
	        //没有openId 重定向到授权页面
	        window.location.replace(
	          "https://open.weixin.qq.com/connect/oauth2/authorize?appid=" +
	            jsonData.only_id +
	            "&redirect_uri=" +
	            encodeURIComponent(window.location.href) +
	            "&response_type=code&scope=snsapi_userinfo&state=STATE#wechat_redirect"
	        );
	      }
	    } catch (e) {
	      console.log(e);
	    }
	  };
