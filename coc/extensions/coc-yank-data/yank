	export const newReducer: Reducer<any> = (state: StoreStructure, action: ActionTypeBase) => {
	    switch(action.type){
	        case LOAD_NEWS:
	            console.log('LOAD news (action in reducer');
	            break;
	        case ADD_NEWS:
	            console.log('ADD news (action in reducer');
	            break;
	        default:
	            return state;
	    }
	}
